Dialogue: 0,0:22:54.14,0:22:56.91,main,All,0000,0000,0000,,!نفلة صغيرة
Dialogue: 0,0:22:57.02,0:23:01.02,sign_33188_278_The_Mystery_of_t,Sign,0000,0000,0000,,{\fad(520,1)}تحدّي ميريوليونا
Dialogue: 0,0:22:58.17,0:23:00.33,main,Mere,0000,0000,0000,,!تحدّي ميريوليونا
Dialogue: 0,0:23:01.41,0:23:03.69,main,Mere,0000,0000,0000,,!حاولا قول ميريوليونا عشر مرات
Dialogue: 0,0:23:03.69,0:23:07.63,main,Asta,0000,0000,0000,,،ميريوليونا، ميريوليونا، ميريوليونا\N—ميريوليونا، ميريوليونا، ميرا
Dialogue: 0,0:23:08.19,0:23:09.68,main,Mere,0000,0000,0000,,!أيّها الأحمق
Dialogue: 0,0:23:10.69,0:23:12.02,main,Mere,0000,0000,0000,,!هذا جزاء الفشل
Dialogue: 0,0:23:12.02,0:23:12.86,main,Mere,0000,0000,0000,,!التالي
Dialogue: 0,0:23:12.86,0:23:15.73,main,Zora,0000,0000,0000,,—ميريوليونا، ميريوليونا، ميريوليونا، ميرو
Dialogue: 0,0:23:17.94,0:23:19.46,main,Mere,0000,0000,0000,,!مثير للشفقة
Dialogue: 0,0:23:19.46,0:23:22.64,main,Asta,0000,0000,0000,,!أرينا كيف نقوم بالأمر أيّتها القائدة ميريوليونا
Dialogue: 0,0:23:22.64,0:23:25.04,main,Mere,0000,0000,0000,,.اسمعا جيّدًا أيّها الطفلان الأحمقان
Dialogue: 0,0:23:25.04,0:23:27.73,main,Mere,0000,0000,0000,,!منطقة المانا: الإطلاق الكامل
Dialogue: 0,0:23:31.75,0:23:32.84,main,Both,0000,0000,0000,,
Dialogue: 0,0:23:32.84,0:23:34.07,main,Mere,0000,0000,0000,,—ميو
Dialogue: 0,0:23:34.07,0:23:36.09,main,Both,0000,0000,0000,,!أيتها القائدة
Dialogue: 0,0:23:36.09,0:23:37.44,italics,Mere,0000,0000,0000,,!مستحيل
Dialogue: 0,0:23:40.33,0:23:43.87,main,Asta,0000,0000,0000,,!يونو! لا تقل لي أنّك تحولت إلى عدو أيضًا
Dialogue: 0,0:23:43.87,0:23:46.32,main,Asta,0000,0000,0000,,!تذكر وعدنا
Dialogue: 0,0:23:46.32,0:23:50.61,main,asta,0000,0000,0000,,في الحلقة القادمة من بلاك كلوفر\N.الصفحة 100، لن نخسر أمامك
Dialogue: 0,0:23:47.57,0:23:54.25,sign_34238_338_Page_2_The_Boys_,Sign,0000,0000,0000,,{\fad(234,1)}الصفحة 100\N\N{\fs18} لن نخسر أمامك
Dialogue: 0,0:23:50.61,0:23:52.53,main,Asta,0000,0000,0000,,!عدم الاستسلام هو سحري
