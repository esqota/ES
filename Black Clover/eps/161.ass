Dialogue: 0,0:16:07.27,0:16:08.57,main,Yuno,0000,0000,0000,,...كلاوس-سينبـ
Dialogue: 0,0:16:28.92,0:16:32.64,main,Yuno,0000,0000,0000,,!أيّها الوغد
Dialogue: 0,0:16:41.73,0:16:44.16,italics,Yuno,0000,0000,0000,,!لا يمكنني قطعه بروح زفير؟
Dialogue: 0,0:16:44.57,0:16:46.19,italics,Yuno,0000,0000,0000,,...لا، إنّه
Dialogue: 0,0:16:46.19,0:16:49.97,italics,Yuno,0000,0000,0000,,!يتجدّد بسرعة كبيرة
Dialogue: 0,0:16:50.96,0:16:54.95,main,Zenon,0000,0000,0000,,...أنت من المستوى صفر إذًا. دعني أريك إذًا
Dialogue: 0,0:16:56.20,0:16:58.75,main,Zenon,0000,0000,0000,,.القليل من قوى الشيطان خاصتي
