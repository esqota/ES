Dialogue: 0,0:15:35.66,0:15:37.54,main,Bell,0000,0000,0000,,!بسرعة! استخدم انغماس الرّوح
Dialogue: 0,0:15:37.83,0:15:40.29,main,Bell,0000,0000,0000,,.إنّه خطير
Dialogue: 0,0:15:50.02,0:15:51.68,italics,Yuno,0000,0000,0000,,!لا يوجد وقت لتفاديها
Dialogue: 0,0:15:59.04,0:16:00.25,italics,Letoile,0000,0000,0000,,...إنّه
Dialogue: 0,0:16:00.25,0:16:01.31,main,Klaus,0000,0000,0000,,!قويّ للغاية فحسب
