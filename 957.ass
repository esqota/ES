Dialogue: 0,0:21:27.68,0:21:30.56,Default,,0000,0000,0000,,!يا أميرة الثّعابين، لدينا مشكلة
Dialogue: 0,0:21:30.56,0:21:34.98,Default,,0000,0000,0000,,!تمّ سحب لقب الشّيتشيبوكاي منك
Dialogue: 0,0:21:35.35,0:21:36.77,Default,,0000,0000,0000,,!أيّتها الجدّة نيون
Dialogue: 0,0:21:36.77,0:21:38.75,Default,,0000,0000,0000,,!ثمّة سفن تابعة للبحريّة على السّاحل
Dialogue: 0,0:21:38.75,0:21:39.94,Default,,0000,0000,0000,,!ماذا قلت؟
Dialogue: 0,0:21:39.94,0:21:42.74,Default,,0000,0000,0000,,!بدؤوا تحرّكاتهم بالفعل؟
Dialogue: 0,0:21:44.53,0:21:46.20,Default,,0000,0000,0000,,!لا تهلعن
Dialogue: 0,0:21:48.62,0:21:49.78,Default,,0000,0000,0000,,!أميرة الثّعابين
Dialogue: 0,0:21:49.87,0:21:52.20,Default,,0000,0000,0000,,...أعتقد أنّهم
Dialogue: 0,0:21:54.00,0:21:55.77,Default,,0000,0000,0000,,!قد نسوا أمرًا
Dialogue: 0,0:21:55.77,0:21:58.56,Default,,0000,0000,0000,,!لا فائدة أيّها الرّئيس باغي
Dialogue: 0,0:21:58.56,0:21:59.96,Default,,0000,0000,0000,,!هل نهرب؟
Dialogue: 0,0:22:00.59,0:22:04.63,Default,,0000,0000,0000,,!بالطّبع سنقاتلهم بطريقة مبهرجة
Dialogue: 0,0:22:09.26,0:22:11.76,Default,,0000,0000,0000,,!أظهروا بعض الجرأة
Dialogue: 0,0:22:11.76,0:22:14.31,Default,,0000,0000,0000,,!أيّها الحمقى
Dialogue: 0,0:22:15.73,0:22:18.35,Default,,0000,0000,0000,,!هذا هو رئيسنا باغي
