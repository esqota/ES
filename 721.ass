Dialogue: 0,0:20:45.30,0:20:47.01,notes,,0,0,0,,!السوط العملاق
Dialogue: 0,0:20:47.26,0:20:48.43,notes,,0,0,0,,!تسلّح التصلّب
Dialogue: 0,0:20:55.58,0:20:57.49,Default,,0,0,0,,!ما هذا؟
Dialogue: 0,0:21:03.57,0:21:07.07,notes,,0,0,0,,!رصاصة النابض
Dialogue: 0,0:21:29.83,0:21:31.76,Default,,0,0,0,,.لقد أوشكتَ على إصابتي
Dialogue: 0,0:21:53.27,0:21:55.36,Default,,0,0,0,,هل من خطب؟
Dialogue: 0,0:21:56.54,0:21:57.29,Default,,0,0,0,,...كلا
Dialogue: 0,0:22:11.07,0:22:13.43,Default,,0,0,0,,!مهلًا، ابقوا في أماكنكم
Dialogue: 0,0:22:13.68,0:22:14.63,Default,,0,0,0,,ماذا؟
Dialogue: 0,0:22:25.38,0:22:28.32,Default,,0,0,0,,!هذا الصوت -\Nما-ما الأمر؟ -
Dialogue: 0,0:22:28.43,0:22:29.24,Default,,0,0,0,,ماذا؟
Dialogue: 0,0:22:30.68,0:22:31.78,Default,,0,0,0,,ما الخطب؟
Dialogue: 0,0:22:37.23,0:22:40.17,Default,,0,0,0,,.كلا، إنّها مُخيلتي فحسب
Dialogue: 0,0:22:42.56,0:22:44.38,Default,,0,0,0,,!بل إنّهُ يتحرّك
Dialogue: 0,0:22:47.20,0:22:49.86,Default,,0,0,0,,...مُحال -\N...جدار الخيوط -
Dialogue: 0,0:22:53.33,0:22:55.94,Default,,0,0,0,,!يتقلّص
Dialogue: 0,0:23:01.38,0:23:04.53,Default,,0,0,0,,.سأجعلك تلحق بلاو بعد قليل
Dialogue: 0,0:23:07.80,0:23:10.86,Default,,0,0,0,,.لنُنهي هذه اللعبة
Dialogue: 0,0:23:11.12,0:23:13.22,Default,,0,0,0,,!!مينغو
Dialogue: 0,0:23:13.22,0:23:14.67,Default,,0,0,0,,!مهلًا
Dialogue: 0,0:23:15.44,0:23:18.27,Default,,0,0,0,,!استمع جيّدًا يا قُبّعة القش-يا
Dialogue: 0,0:23:24.32,0:23:28.17,Default,,0,0,0,,!خطّة القضاء على المتوحّش دوفلامينغو بدأت أخيرًا
Dialogue: 0,0:23:28.20,0:23:33.30,Default,,0,0,0,,!الجدار المُتقلّص يقطع دريسروزا! لقد تبقّت ساعة واحدة فقط
Dialogue: 0,0:23:33.66,0:23:36.14,Default,,0,0,0,,،حينما يبدو أنّ كُل أمل قد خاب
Dialogue: 0,0:23:36.14,0:23:40.19,Default,,0,0,0,,!هجومٌ عنيد يستغل فرصة وحيدة يظهر
Dialogue: 0,0:23:40.19,0:23:41.57,Default,,0,0,0,,!في الحلقة القادمة من ون بيس
Dialogue: 0,0:23:41.57,0:23:44.06,Default,,0,0,0,,!نصل الصلابة\N!هجوم سكّين غامّا المُضاد
Dialogue: 0,0:23:46.07,0:23:50.07,title,,0,0,0,,{\pos(640,635.4)}!نصل الصلابة\N\N\N\N\N!هجوم سكّين غامّا المُضاد
Dialogue: 0,0:23:44.34,0:23:46.54,Default,,0,0,0,,!سأُصبحُ ملكَ القراصنة
