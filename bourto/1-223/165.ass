Dialogue: 0,0:00:07.31,0:00:08.98,Main,,0000,0000,0000,,.أهدرنا الكثير من الوقت
Dialogue: 0,0:00:09.38,0:00:11.08,Main,,0000,0000,0000,,.لا يمكننا أن ندعهما يوسّعان المسافة بيننا أكثر
Dialogue: 0,0:00:11.54,0:00:12.61,Main,,0000,0000,0000,,.لنسرع ونلحق بهما
Dialogue: 0,0:00:12.94,0:00:13.51,Main,,0000,0000,0000,,!أجل
Dialogue: 0,0:00:14.98,0:00:17.28,Main,,0000,0000,0000,,.لكن علينا أن نتقدّم ببالغ الحذر
Dialogue: 0,0:00:17.98,0:00:19.14,Main,,0000,0000,0000,,،من أجل هدفهما
Dialogue: 0,0:00:19.61,0:00:22.01,Main,,0000,0000,0000,,.لن يتردّدا أبدًا للمخاطرة بحياتهما
Dialogue: 0,0:00:24.68,0:00:25.64,Main,,0000,0000,0000,,ما الخطب؟
Dialogue: 0,0:00:25.84,0:00:26.51,Main,,0000,0000,0000,,!لنذهب
Dialogue: 0,0:00:28.94,0:00:29.78,Main,,0000,0000,0000,,!حاضر
Dialogue: 0,0:00:47.18,0:00:48.78,Main,,0000,0000,0000,,.وفيت بكلامي
Dialogue: 0,0:00:49.54,0:00:51.94,Main,,0000,0000,0000,,.سأدعك تعيش لوحدك
Dialogue: 0,0:00:55.61,0:00:57.18,Main,,0000,0000,0000,,أرض السّديم إذًا؟
Dialogue: 0,0:00:58.11,0:01:00.38,Main,,0000,0000,0000,,.كان يجب أن يظلّا عند حدّهما
Dialogue: 0,0:01:06.04,0:01:07.11,Main,,0000,0000,0000,,!كاكوي
Dialogue: 0,0:01:08.98,0:01:09.88,Main,,0000,0000,0000,,!ماروي
Dialogue: 0,0:01:36.44,0:01:38.58,Main,,0000,0000,0000,,.يبدو أنّ الخلايا لا تزال ثابتة
Dialogue: 0,0:01:40.11,0:01:41.34,Main,,0000,0000,0000,,.اقتربنا من الحدود
Dialogue: 0,0:01:41.61,0:01:42.71,Main,,0000,0000,0000,,أيجب أن نسرّع من وتيرتنا؟
Dialogue: 0,0:01:44.58,0:01:46.41,Main,,0000,0000,0000,,.هيروغا... كل
Dialogue: 0,0:01:47.84,0:01:48.71,Main,,0000,0000,0000,,.أنا بخير
Dialogue: 0,0:01:50.41,0:01:53.01,Main,,0000,0000,0000,,.ستعيق مهمّتنا إلّم تأكل أو تشرب
Dialogue: 0,0:01:54.71,0:01:55.44,Main,,0000,0000,0000,,.لذا، عليك أن تأكل
Dialogue: 0,0:02:03.44,0:02:05.41,Main,,0000,0000,0000,,.ما عدت بحاجة لتقسيم الطّعام إلى أربعة أقسام كما تعلم
Dialogue: 0,0:02:12.41,0:02:14.71,Main,,0000,0000,0000,,.بقي نحن الاثنان فقط الآن
Dialogue: 0,0:02:15.61,0:02:16.91,Main,,0000,0000,0000,,.توقّعنا حدوث هذا
Dialogue: 0,0:02:20.68,0:02:24.31,Main,,0000,0000,0000,,،فقد أوتنا أرض السّديم كيتامى وأطعمتنا {he says Kasumi no Kuni}
Dialogue: 0,0:02:24.48,0:02:26.61,Main,,0000,0000,0000,,.ومنحتنا طريقتنا في الحياة كشينوبي
Dialogue: 0,0:02:27.78,0:02:30.48,Main,,0000,0000,0000,,.وقد آن الأوان لنردّ المعروف لهم
Dialogue: 0,0:02:31.14,0:02:32.41,Main,,0000,0000,0000,,.أعلم ذلك
Dialogue: 0,0:02:34.01,0:02:35.08,Main,,0000,0000,0000,,.علينا أن نأكل فحسب
Dialogue: 0,0:02:35.48,0:02:37.41,Main,,0000,0000,0000,,.سيسعدهما ذلك
Dialogue: 0,0:02:39.68,0:02:40.51,Main,,0000,0000,0000,,.معك حقّ
Dialogue: 0,0:02:49.44,0:02:50.28,Main,,0000,0000,0000,,.لنذهب
Dialogue: 0,0:04:26.37,0:04:30.90,ep title,,0000,0000,0000,,{\fad(100,100)}{\blur8\bord5\fs37}{\i1}واجب الرّباعي{\i0}\N
Dialogue: 0,0:04:37.60,0:04:40.03,Main,,0000,0000,0000,,!مات يوغا لإيقافهم
Dialogue: 0,0:04:40.37,0:04:41.27,Main,,0000,0000,0000,,!—يستحيل أنّ أحدًا
Dialogue: 0,0:04:41.33,0:04:42.87,Main,,0000,0000,0000,,.يبدو أنّه لم يقوَ على إيقافهم
Dialogue: 0,0:04:45.67,0:04:47.60,Main,,0000,0000,0000,,ما يعني أنّ دوري قد حان؟
Dialogue: 0,0:05:07.20,0:05:08.73,Main,,0000,0000,0000,,.لا ندري ما قد يحاولا فعله
Dialogue: 0,0:05:09.10,0:05:11.20,Main,,0000,0000,0000,,سوف نواجههما أوّلًا، قبل أن يتسنّى لهما استخدام
Dialogue: 0,0:05:11.27,0:05:12.40,Main,,0000,0000,0000,,.تقنيّة مزعجة أخرى
Dialogue: 0,0:05:13.10,0:05:14.60,Main,,0000,0000,0000,,.وأنتم ستستريحون
Dialogue: 0,0:05:15.10,0:05:15.80,Main,,0000,0000,0000,,.نعم
Dialogue: 0,0:05:22.23,0:05:23.00,Main,,0000,0000,0000,,!لن أدعك
Dialogue: 0,0:05:29.70,0:05:31.10,Main,,0000,0000,0000,,!يجدر أنّهما لا يزالان في الجوار
Dialogue: 0,0:05:36.50,0:05:37.33,Main,,0000,0000,0000,,!ما هذا؟
Dialogue: 0,0:05:37.43,0:05:38.40,Main,,0000,0000,0000,,حمأة؟
Dialogue: 0,0:05:47.47,0:05:48.97,Main,,0000,0000,0000,,!ذلك المظهر
Dialogue: 0,0:05:53.97,0:05:55.10,Main,,0000,0000,0000,,!—ساقاي
Dialogue: 0,0:05:55.87,0:05:57.00,Main,,0000,0000,0000,,!فنّ نينجا السّديم
Dialogue: 0,0:05:57.20,0:05:59.57,Main,,0000,0000,0000,,!التقنيّة المحظورة: الإبادة الجهنّميّة
Dialogue: 0,0:06:08.63,0:06:10.47,Main,,0000,0000,0000,,!من بين كلّ الخدع الغريبة
Dialogue: 0,0:06:14.33,0:06:15.30,Main,,0000,0000,0000,,!سحقًا
Dialogue: 0,0:06:15.37,0:06:16.57,Main,,0000,0000,0000,,!بوروتو، انبطح
Dialogue: 0,0:06:17.20,0:06:18.63,Main,,0000,0000,0000,,!أسلوب البرق: كرة البرق
Dialogue: 0,0:06:20.33,0:06:22.13,Main,,0000,0000,0000,,!لا يدعنا نستخدم الجيتسو
Dialogue: 0,0:06:25.10,0:06:26.40,Main,,0000,0000,0000,,!أفاعي الظلّ الضّاربة المتعدّدة
Dialogue: 0,0:06:33.20,0:06:34.37,Main,,0000,0000,0000,,!خلايا هاشيراما
Dialogue: 0,0:06:35.27,0:06:36.90,Main,,0000,0000,0000,,!اذهب فحسب يا أساكا
Dialogue: 0,0:06:37.40,0:06:39.23,Main,,0000,0000,0000,,!سأتكفّل بهم
Dialogue: 0,0:06:41.10,0:06:43.83,Main,,0000,0000,0000,,!نعيش في سبيل المهمّة فقط
Dialogue: 0,0:06:45.53,0:06:46.47,Main,,0000,0000,0000,,!انتظر
Dialogue: 0,0:06:57.13,0:06:59.23,Main,,0000,0000,0000,,.كافحوا ما شئتم. فعبثًا تحاولون
Dialogue: 0,0:06:59.80,0:07:03.27,Main,,0000,0000,0000,,!سوف تجرّ أرواح العالم السفليّ كلّ شيء إلى الأسفل
Dialogue: 0,0:07:06.97,0:07:08.10,Main,,0000,0000,0000,,!—أيّها الـ
Dialogue: 0,0:07:09.00,0:07:11.00,Main,,0000,0000,0000,,وماذا ستفعل بذلك؟
Dialogue: 0,0:07:11.27,0:07:15.30,Main,,0000,0000,0000,,سأمحو تلك الابتسامة المتعجرفة \N!عن وجهك بالرّاسينغان خاصّتي
Dialogue: 0,0:07:15.67,0:07:17.90,Main,,0000,0000,0000,,!حاول إن كنت تستطيع
Dialogue: 0,0:07:18.23,0:07:20.17,Main,,0000,0000,0000,,!لا تستهن بي
Dialogue: 0,0:07:24.53,0:07:26.27,Main,,0000,0000,0000,,!توقّف يا بوروتو
Dialogue: 0,0:07:26.47,0:07:28.10,Main,,0000,0000,0000,,!لا تحاولي إيقافي يا سارادا
Dialogue: 0,0:07:28.20,0:07:29.60,Main,,0000,0000,0000,,!ليس ذلك
Dialogue: 0,0:07:36.63,0:07:38.30,Main,,0000,0000,0000,,!سرعة غرقك مختلفة
Dialogue: 0,0:07:38.43,0:07:41.50,Main,,0000,0000,0000,,!كلّما اقتربت منه، زاد مفعول تقنيّته
Dialogue: 0,0:07:41.70,0:07:42.57,Main,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:07:45.43,0:07:47.10,Main,,0000,0000,0000,,.ملاحظة فطنة
Dialogue: 0,0:07:47.27,0:07:49.70,Main,,0000,0000,0000,,.أردتُ أن يقترب منّي أكثر
Dialogue: 0,0:07:49.83,0:07:51.23,Main,,0000,0000,0000,,.لكن، لا يهمّ
Dialogue: 0,0:07:51.63,0:07:53.47,Main,,0000,0000,0000,,!—في تلك الحالة
Dialogue: 0,0:07:57.13,0:07:59.43,Main,,0000,0000,0000,,!سحقًا، إنّها أشبه بحفرة ليث عفرين\N
Dialogue: 0,0:08:01.77,0:08:03.67,Main,,0000,0000,0000,,!علينا الابتعاد عنه
Dialogue: 0,0:08:05.47,0:08:07.20,Main,,0000,0000,0000,,!بوروتو، أعطني يدك
Dialogue: 0,0:08:13.33,0:08:14.20,Main,,0000,0000,0000,,!بوروتو
Dialogue: 0,0:08:14.80,0:08:18.07,Main,,0000,0000,0000,,.مهما حاولتم، فات الأوان بالفعل
Dialogue: 0,0:08:18.80,0:08:21.97,Main,,0000,0000,0000,,!سأسحبكم إلى الجحيم معي
Dialogue: 0,0:08:23.20,0:08:24.13,Main,,0000,0000,0000,,!محال
Dialogue: 0,0:08:24.33,0:08:25.17,Main,,0000,0000,0000,,أنت أيضًا؟
Dialogue: 0,0:08:25.70,0:08:27.23,Main,,0000,0000,0000,,هل أدركتم ذلك الآن؟
Dialogue: 0,0:08:28.07,0:08:30.80,Main,,0000,0000,0000,,...أنا من سينقلكم
Dialogue: 0,0:08:31.07,0:08:33.37,Main,,0000,0000,0000,,!إلى العالم السفليّ
Dialogue: 0,0:08:33.80,0:08:35.47,Main,,0000,0000,0000,,!ما بالكم على أيّ حال؟
Dialogue: 0,0:08:36.10,0:08:38.97,Main,,0000,0000,0000,,!لمَ أنتم متلهّفون لإهدار حياتكم؟
Dialogue: 0,0:08:39.10,0:08:40.63,Main,,0000,0000,0000,,!لسنا نهدرها
Dialogue: 0,0:08:40.90,0:08:42.07,Main,,0000,0000,0000,,!بل ننقذ حياة النّاس
Dialogue: 0,0:08:42.17,0:08:43.23,Main,,0000,0000,0000,,تنقذون حياة النّاس؟
Dialogue: 0,0:08:44.33,0:08:47.80,Main,,0000,0000,0000,,!بموتي، سأنقذ حياة أحد آخر
Dialogue: 0,0:08:48.07,0:08:50.90,Main,,0000,0000,0000,,أهذه مهمّتك؟
Dialogue: 0,0:08:52.37,0:08:55.57,Main,,0000,0000,0000,,!ألا تظنّ أنّ أمرًا كهذا ضرب من الجنون؟
Dialogue: 0,0:08:57.73,0:09:01.00,Main,,0000,0000,0000,,.لن تفهموا أبدًا
Dialogue: 0,0:09:02.20,0:09:03.07,Main,,0000,0000,0000,,...كيف أنا
Dialogue: 0,0:09:03.60,0:09:06.17,Main,,0000,0000,0000,,!كلّا، بل كيف أشعر أنا وإخوتي
Dialogue: 0,0:09:12.43,0:09:13.17,Main - flashback,,0000,0000,0000,,.هيروغا
Dialogue: 0,0:09:13.57,0:09:14.33,Main - flashback,,0000,0000,0000,,نعم؟
Dialogue: 0,0:09:15.17,0:09:17.83,Main - flashback,,0000,0000,0000,,.منتصف أرض الصّمت قريب في الأمام
Dialogue: 0,0:09:18.57,0:09:19.63,Main - flashback,,0000,0000,0000,,.لا ترخ دفاعك
Dialogue: 0,0:09:19.97,0:09:21.33,Main - flashback,,0000,0000,0000,,!مفاجأة
Dialogue: 0,0:09:22.60,0:09:25.83,Main - flashback,,0000,0000,0000,,!فكّرت أن نأكلها جميعًا، لذا وفّرتها
Dialogue: 0,0:09:26.47,0:09:28.43,Main - flashback,,0000,0000,0000,,.يوروغا، هذه خوختك
Dialogue: 0,0:09:28.70,0:09:29.63,Main - flashback,,0000,0000,0000,,!كلها أنت
Dialogue: 0,0:09:30.27,0:09:32.67,Main - flashback,,0000,0000,0000,,هيروغا، هل تريد انتهاك القاعدة؟
Dialogue: 0,0:09:33.13,0:09:34.73,Main - flashback,,0000,0000,0000,,،كيفما كان الوضع
Dialogue: 0,0:09:34.80,0:09:36.70,Main - flashback,,0000,0000,0000,,يجب أن نقسّم كلّ شيء إلى أربعة أقسام، صحيح؟
Dialogue: 0,0:09:42.80,0:09:46.40,Main - flashback,,0000,0000,0000,,كانت هنالك شجرة خوخ عملاقة خلف الميتم، صحيح؟
Dialogue: 0,0:09:46.50,0:09:47.33,Main - flashback,,0000,0000,0000,,.أجل
Dialogue: 0,0:09:47.40,0:09:49.67,Main - flashback,,0000,0000,0000,,.كنّا نختبئ وراءها ونأكل حتّى لا يجدونا
Dialogue: 0,0:09:50.20,0:09:52.37,Main - flashback,,0000,0000,0000,,أتظنّون أنّ تلك الشّجرة لا تزال منتصبة؟
Dialogue: 0,0:09:52.87,0:09:55.70,Main - flashback,,0000,0000,0000,,...إن أنجزنا هذه المهمّة وعدنا إلى الدّيار أحياء
Dialogue: 0,0:09:55.87,0:09:57.97,Main - flashback,,0000,0000,0000,,!مرّ وقت طويل، لكن دعونا نعد إلى هناك
Dialogue: 0,0:09:58.30,0:09:59.47,Main - flashback,,0000,0000,0000,,.يبدو هذا جيّدًا
Dialogue: 0,0:09:59.90,0:10:02.97,Main - flashback,,0000,0000,0000,,.هذا إن استطعنا العودة جميعًا
Dialogue: 0,0:10:03.23,0:10:05.90,Main - flashback,,0000,0000,0000,,!لن يصل دورك مع ذلك
Dialogue: 0,0:10:06.00,0:10:07.90,Main - flashback,,0000,0000,0000,,وما أدراك؟
Dialogue: 0,0:10:08.90,0:10:10.30,Main - flashback,,0000,0000,0000,,،فور أن ندخل القرية
Dialogue: 0,0:10:10.37,0:10:12.50,Main - flashback,,0000,0000,0000,,.علينا أن نجمع معلومات عن خلايا هاشيراما
Dialogue: 0,0:10:12.83,0:10:13.43,Main - flashback,,0000,0000,0000,,.أجل
Dialogue: 0,0:10:37.03,0:10:38.77,Main - flashback,,0000,0000,0000,,.نعرف من يملكها الآن
Dialogue: 0,0:10:39.37,0:10:40.73,Main - flashback,,0000,0000,0000,,.لنباشر
Dialogue: 0,0:10:40.83,0:10:41.57,Main - flashback,,0000,0000,0000,,.أجل
Dialogue: 0,0:10:56.07,0:10:57.33,Main - flashback,,0000,0000,0000,,!إنّه حاجز قويّ
Dialogue: 0,0:10:57.97,0:11:01.73,Main - flashback,,0000,0000,0000,,.إن اقتحمناه بالقوّة، لن يكشفوا أمرنا فحسب... بل وسنموت
Dialogue: 0,0:11:03.43,0:11:06.50,Main - flashback,,0000,0000,0000,,لكن إن أهدرنا الوقت، سيأتي أولئك \N.الأشخاص من قرية الورق إلى هنا
Dialogue: 0,0:11:06.67,0:11:08.83,Main - flashback,,0000,0000,0000,,!أجل، أجل... تنحّوا جانبًا
Dialogue: 0,0:11:09.10,0:11:09.90,Main - flashback,,0000,0000,0000,,!يوروغا
Dialogue: 0,0:11:12.43,0:11:15.30,Main - flashback,,0000,0000,0000,,!حسنٌ... أظنّ أنّ دوري قد حان
Dialogue: 0,0:11:17.27,0:11:19.23,Main - flashback,,0000,0000,0000,,.اسمع، أسدني معروفًا
Dialogue: 0,0:11:20.37,0:11:23.30,Main - flashback,,0000,0000,0000,,.خذ هذه وادفنها تحت شجرة الخوخ
Dialogue: 0,0:11:23.67,0:11:27.23,Main - flashback,,0000,0000,0000,,!هكذا سأستطيع أن أرى أرضنا وهي تزدهر وتسعد
Dialogue: 0,0:11:30.57,0:11:32.13,Main - flashback,,0000,0000,0000,,!نعتمد عليك
Dialogue: 0,0:11:38.00,0:11:39.47,Main - flashback,,0000,0000,0000,,...فنّ نينجا السّديم
Dialogue: 0,0:11:39.53,0:11:41.80,Main - flashback,,0000,0000,0000,,!التقنيّة المحظورة: بوّابة الحياة الآخرة
Dialogue: 0,0:12:05.83,0:12:06.77,Main - flashback,,0000,0000,0000,,!يوروغا
Dialogue: 0,0:12:07.73,0:12:08.50,Main - flashback,,0000,0000,0000,,.لنذهب
Dialogue: 0,0:12:21.83,0:12:22.63,Main - flashback,,0000,0000,0000,,...وعدنا
Dialogue: 0,0:12:23.50,0:12:25.50,Main - flashback,,0000,0000,0000,,هل فهمته؟
Dialogue: 0,0:12:27.03,0:12:29.73,Main - flashback,,0000,0000,0000,,عدني... في أرضنا... اتّفقنا؟
Dialogue: 0,0:12:35.63,0:12:38.63,Main,,0000,0000,0000,,.قطعتُ وعدًا ليوروغا بأن أعيده إلى الدّيار
Dialogue: 0,0:12:40.03,0:12:41.17,Main,,0000,0000,0000,,.فهمت
Dialogue: 0,0:12:41.97,0:12:43.90,Main,,0000,0000,0000,,!...إذًا من مات آخر مرّة كان
Dialogue: 0,0:12:47.07,0:12:48.40,Main,,0000,0000,0000,,.لم يكن يوروغا فحسب
Dialogue: 0,0:12:49.07,0:12:51.43,Main,,0000,0000,0000,,.أتمّ يوغا واجبه أيضًا
Dialogue: 0,0:12:52.80,0:12:54.57,Main,,0000,0000,0000,,!وكذلك سأفعل
Dialogue: 0,0:13:05.80,0:13:08.33,Main,,0000,0000,0000,,،يوجد في الدّيار الكثير من الأطفال الجياع
Dialogue: 0,0:13:08.40,0:13:09.83,Main,,0000,0000,0000,,.الّذين يشبهوننا كثيرًا
Dialogue: 0,0:13:10.16,0:13:13.23,Main,,0000,0000,0000,,!إن حصلنا على خلايا هاشيراما، يمكننا إنقاذ حياتهم
Dialogue: 0,0:13:14.20,0:13:17.60,Main,,0000,0000,0000,,!خلايا هاشيراما ليست بشيء متاح للاستخدام ببساطة
Dialogue: 0,0:13:17.97,0:13:21.17,Main,,0000,0000,0000,,!هل تعرف ما هي أساسًا؟
Dialogue: 0,0:13:21.57,0:13:23.17,Main,,0000,0000,0000,,!لست بحاجة لأعرف
Dialogue: 0,0:13:23.50,0:13:26.07,Main,,0000,0000,0000,,،إن قالت أرضنا أنّها بحاجة لها
Dialogue: 0,0:13:26.13,0:13:29.23,Main,,0000,0000,0000,,!يقضي واجبنا بإتمام المهمّة. هذا كلّ شيء
Dialogue: 0,0:13:29.67,0:13:32.20,Main,,0000,0000,0000,,!ذلك أمر سخيف وعابث
Dialogue: 0,0:13:32.40,0:13:35.53,Main,,0000,0000,0000,,!وكأنّهم يأمرونكم أن تذهبوا لتقوا حتفكم
Dialogue: 0,0:13:36.37,0:13:39.67,Main,,0000,0000,0000,,.أوتنا أرضنا كيتامى
Dialogue: 0,0:13:39.73,0:13:41.93,Main,,0000,0000,0000,,.وأعطتنا وجبات دافئة
Dialogue: 0,0:13:44.27,0:13:46.97,Main,,0000,0000,0000,,هل جرّبتم أن تُحتضروا من الجوع يومًا؟
Dialogue: 0,0:13:47.77,0:13:50.37,Main,,0000,0000,0000,,أسبق وشعرتم بإحساس الطّعام
Dialogue: 0,0:13:50.43,0:13:53.37,Main,,0000,0000,0000,,!وهو ينصبّ على معدتكم الفارغة والشّبيهة بالقشرة؟
Dialogue: 0,0:14:02.17,0:14:04.27,Main,,0000,0000,0000,,لا يسعكم حتّى أن تتخيّلوا ذلك
Dialogue: 0,0:14:04.33,0:14:07.30,Main,,0000,0000,0000,,!لأنّكم عشتم حياة هانئة في دولة عظيمة
Dialogue: 0,0:14:13.87,0:14:15.53,Main,,0000,0000,0000,,!...حقًّا، على هذا الحال
Dialogue: 0,0:14:16.70,0:14:19.13,Main,,0000,0000,0000,,!فقط لو أنّ ذراعيّ كانتا طليقتين
Dialogue: 0,0:14:20.83,0:14:21.70,Main,,0000,0000,0000,,!سحقًا
Dialogue: 0,0:14:33.97,0:14:37.40,Main,,0000,0000,0000,,...يبدو أنّنا ضُلّلنا
Dialogue: 0,0:14:37.93,0:14:39.30,Main,,0000,0000,0000,,!عمّاذا تتحدّث؟
Dialogue: 0,0:14:39.63,0:14:43.17,Main,,0000,0000,0000,,!على هذا المعدّل، سيكون أوّل من يغرق
Dialogue: 0,0:14:47.73,0:14:49.60,Main,,0000,0000,0000,,...ما يعني أنّه إن مات
Dialogue: 0,0:14:49.67,0:14:51.20,Main,,0000,0000,0000,,!—فهذه التّقنيّة الدّنيئة
Dialogue: 0,0:14:53.93,0:14:55.43,Main,,0000,0000,0000,,هل أدركتم هذا أخيرًا؟
Dialogue: 0,0:14:56.20,0:14:57.37,Main,,0000,0000,0000,,ماذا تقصد؟
Dialogue: 0,0:14:57.70,0:14:59.13,Main,,0000,0000,0000,,أيجب أن يكون أوّل من—؟
Dialogue: 0,0:14:59.33,0:15:01.70,Main,,0000,0000,0000,,!إنّه العيب الوحيد لهذه التقنيّة
Dialogue: 0,0:15:02.37,0:15:05.33,Main,,0000,0000,0000,,!مستخدم التقنيّة هو أوّل من يموت، أي أنا
Dialogue: 0,0:15:06.87,0:15:10.20,Main,,0000,0000,0000,,!إذًا، كنت تتفاخر فحسب بقولك أنّك ستسحبنا معك
Dialogue: 0,0:15:10.47,0:15:12.07,Main,,0000,0000,0000,,!كان هذا قصد كسب الوقت فحسب
Dialogue: 0,0:15:12.97,0:15:14.90,Main,,0000,0000,0000,,!لكنّ ذلك يكفي وأكثر
Dialogue: 0,0:15:22.53,0:15:24.23,Main,,0000,0000,0000,,.سيبزغ الفجر قريبًا
Dialogue: 0,0:15:27.13,0:15:29.07,Main,,0000,0000,0000,,...وفور أن تبدأ الشّمس في الطّلوع
Dialogue: 0,0:15:29.57,0:15:32.87,Main,,0000,0000,0000,,.سيكون أساكا عبر الحدود بالفعل
Dialogue: 0,0:15:34.57,0:15:37.53,Main,,0000,0000,0000,,!إذًا كان هذا حافزك من البداية؟
Dialogue: 0,0:15:37.90,0:15:40.50,Main,,0000,0000,0000,,،اضطررنا لسلك الطّريق الطّويل حتّى الآن
Dialogue: 0,0:15:40.57,0:15:43.70,Main,,0000,0000,0000,,.لأنّنا كنّا قلقين على خلايا هاشيراما
Dialogue: 0,0:15:44.67,0:15:49.27,Main,,0000,0000,0000,,!ومن الآن فصاعدًا، يمكنه أن يأخذ الطّريق الأقصر إلى الدّيار
Dialogue: 0,0:15:54.30,0:15:56.87,Main,,0000,0000,0000,,!فزنا
Dialogue: 0,0:15:58.77,0:15:59.47,Main,,0000,0000,0000,,!أنت
Dialogue: 0,0:16:25.17,0:16:26.83,Main,,0000,0000,0000,,!أنت، هل أنت بخير؟
Dialogue: 0,0:16:27.07,0:16:27.87,Main,,0000,0000,0000,,!بوروتو
Dialogue: 0,0:16:28.30,0:16:30.13,Main,,0000,0000,0000,,!بوروتو، لا تلق بالًا له
Dialogue: 0,0:16:30.57,0:16:33.90,Main,,0000,0000,0000,,!علينا التّفكير في طريقة للخروج من هنا بأسرع وقت
Dialogue: 0,0:16:34.83,0:16:37.23,Main,,0000,0000,0000,,.يوروغا... يوغا
Dialogue: 0,0:16:38.33,0:16:39.93,Main,,0000,0000,0000,,.سأنضمّ إليكما قريبًا
Dialogue: 0,0:16:41.00,0:16:44.13,Main,,0000,0000,0000,,!لنلتقِ... تحت شجرة الخوخ
Dialogue: 0,0:16:44.30,0:16:45.83,Main,,0000,0000,0000,,!كفّ عن التكلّم بجنون
Dialogue: 0,0:16:46.20,0:16:49.43,Main,,0000,0000,0000,,!لا بدّ من وجود طريقة أخرى
Dialogue: 0,0:16:49.97,0:16:52.87,Main,,0000,0000,0000,,!أسرع وحرّر هذه التقنيّة السّخيفة فحسب
Dialogue: 0,0:16:53.43,0:16:55.83,Main,,0000,0000,0000,,!بوروتو، لا تقترب منه
Dialogue: 0,0:16:57.00,0:16:58.60,Main,,0000,0000,0000,,!كافح ما شئت
Dialogue: 0,0:16:58.67,0:17:00.33,Main,,0000,0000,0000,,!بقي القليل فقط
Dialogue: 0,0:17:00.70,0:17:03.73,Main,,0000,0000,0000,,!يجب أن أكسب القليل من الوقت
Dialogue: 0,0:17:06.60,0:17:10.40,Main,,0000,0000,0000,,!هدفنا الوحيد هو استعادة خلايا هاشيراما، صحيح؟
Dialogue: 0,0:17:11.10,0:17:13.37,Main,,0000,0000,0000,,!أعلم
Dialogue: 0,0:17:14.90,0:17:18.80,Main,,0000,0000,0000,,!تابعوا الجدال هكذا إلى أن تطلع الشّمس
Dialogue: 0,0:17:19.60,0:17:21.70,Main,,0000,0000,0000,,!إن كان يريد الموت فدعه
Dialogue: 0,0:17:22.03,0:17:24.27,Main,,0000,0000,0000,,!التّعاطف في غير محلّه سيقتلك فحسب
Dialogue: 0,0:17:24.47,0:17:26.27,Main,,0000,0000,0000,,!موغينو-سان محقّ
Dialogue: 0,0:17:26.97,0:17:29.20,Main,,0000,0000,0000,,!هل تقولان أن أشاهده وهو يموت فحسب؟
Dialogue: 0,0:17:29.87,0:17:33.73,Main,,0000,0000,0000,,!لا بدّ أنّه يفكّر أنّ هذا خياره الوحيد
Dialogue: 0,0:17:34.23,0:17:36.77,Main,,0000,0000,0000,,!لا تفترض وتتسرّع بالاستنتاج عنّي
Dialogue: 0,0:17:38.83,0:17:41.83,Main,,0000,0000,0000,,...أنت تشفق عليّ الآن فقط لأنّك
Dialogue: 0,0:17:41.90,0:17:46.10,Main,,0000,0000,0000,,لا تريد أن تشعر بالنّدم والمعاناة لاحقًا
Dialogue: 0,0:17:46.17,0:17:48.67,Main,,0000,0000,0000,,!لتركك لي أموت
Dialogue: 0,0:17:49.40,0:17:52.97,Main,,0000,0000,0000,,.هكذا سيفكّر شقيّ مدلّل من دولة عظيمة
Dialogue: 0,0:17:53.07,0:17:54.37,Main,,0000,0000,0000,,!وأنت كذلك
Dialogue: 0,0:17:54.90,0:17:57.17,Main,,0000,0000,0000,,!لا تفترض أو تتسرّع بالاستنتاج عنّي
Dialogue: 0,0:18:00.67,0:18:03.50,Main,,0000,0000,0000,,!بوروتو! أخبرتك ألّا تقترب منه
Dialogue: 0,0:18:09.03,0:18:12.47,Main,,0000,0000,0000,,!اقترب! يمكنني أن أسحبك معي
Dialogue: 0,0:18:12.67,0:18:14.43,Main,,0000,0000,0000,,!لا أنوي الموت
Dialogue: 0,0:18:14.50,0:18:17.30,Main,,0000,0000,0000,,!لكن أخبرك أنت ألّا تموت
Dialogue: 0,0:18:18.23,0:18:20.07,Main,,0000,0000,0000,,!بوروتو، إيّاك
Dialogue: 0,0:18:20.20,0:18:21.63,Main,,0000,0000,0000,,!بوروتو، توقّف
Dialogue: 0,0:18:21.83,0:18:22.60,Main,,0000,0000,0000,,!بوروتو
Dialogue: 0,0:18:24.40,0:18:27.50,Main,,0000,0000,0000,,!لنتوقّف عن كلّ هذه الحماقات
Dialogue: 0,0:18:28.13,0:18:30.40,Main,,0000,0000,0000,,!ابتعد أيّها الغبيّ
Dialogue: 0,0:18:34.97,0:18:37.07,Main,,0000,0000,0000,,ما الّذي يجري هنا؟
Dialogue: 0,0:18:38.10,0:18:39.07,Main,,0000,0000,0000,,!أوموي-سان؟
Dialogue: 0,0:18:39.43,0:18:41.37,Main,,0000,0000,0000,,!لا تقترب من هذا المستنقع
Dialogue: 0,0:18:44.00,0:18:45.23,Main,,0000,0000,0000,,ماذا حدث؟
Dialogue: 0,0:18:47.87,0:18:51.20,Main,,0000,0000,0000,,.ذاك الشّخص سابقًا لم يكن عاديًّا
Dialogue: 0,0:18:52.23,0:18:54.93,Main,,0000,0000,0000,,.قتل تابعاي
Dialogue: 0,0:18:56.60,0:19:01.47,Main,,0000,0000,0000,,.واكتشف أنّ خلايا هاشيراما بحوزة شينوبي السّديم
Dialogue: 0,0:19:01.60,0:19:02.73,Main,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:19:03.27,0:19:05.43,Main,,0000,0000,0000,,إذًا، لم يأت إلى هنا بعد؟
Dialogue: 0,0:19:06.30,0:19:11.00,Main,,0000,0000,0000,,أنت، قلت أنّه سلك الطّريق الأقصر إلى أرضكم، صحيح؟
Dialogue: 0,0:19:11.53,0:19:15.80,Main,,0000,0000,0000,,إن كان الشّخص الّذي قاتله أوموي-سان \N...متوجّه إلى أرض السّديم مباشرةً
Dialogue: 0,0:19:16.83,0:19:19.40,Main,,0000,0000,0000,,.فمن المرجّح جدًّا أن يتواجها مع بعضهما في نقطةٍ ما
Dialogue: 0,0:19:20.03,0:19:20.97,Main,,0000,0000,0000,,!أساكا
Dialogue: 0,0:19:22.87,0:19:25.07,Main,,0000,0000,0000,,يبدو أنّ الوضع تغيّر إذًا؟
Dialogue: 0,0:19:25.53,0:19:27.83,Main,,0000,0000,0000,,!لـ-لا أصدّقه
Dialogue: 0,0:19:28.20,0:19:31.40,Main,,0000,0000,0000,,!إنّه يكذب لأنّه يريد خلايا هاشيراما
Dialogue: 0,0:19:33.90,0:19:36.87,Main,,0000,0000,0000,,!إذًا اذهب وتأكّد بأمّ عينيك
Dialogue: 0,0:19:50.20,0:19:51.27,Main,,0000,0000,0000,,!تحرير
Dialogue: 0,0:20:20.67,0:20:21.70,Main,,0000,0000,0000,,!سحقًا
Dialogue: 0,0:20:22.80,0:20:24.27,Main,,0000,0000,0000,,!أنت طائش بحقّ
Dialogue: 0,0:20:25.80,0:20:29.13,Main,,0000,0000,0000,,!معه حقّ! أمكنك أن تموت
Dialogue: 0,0:20:30.23,0:20:31.47,Main,,0000,0000,0000,,.ما كان باليد حيلة
Dialogue: 0,0:20:32.77,0:20:35.17,Main,,0000,0000,0000,,.تحرّك جسدي من نفسه
Dialogue: 0,0:20:37.60,0:20:38.73,Main,,0000,0000,0000,,!هذا من طبعك يا بوروتو
Dialogue: 0,0:20:46.40,0:20:48.53,Main,,0000,0000,0000,,.هذا لا يعني أنّي أثق بكم
Dialogue: 0,0:20:52.13,0:20:54.53,Main,,0000,0000,0000,,!بل فقط إلى أن أحرص على سلامة أساكا
Dialogue: 0,0:20:58.93,0:21:00.50,Main,,0000,0000,0000,,!يا له من شخص ظريف
Dialogue: 0,0:21:02.17,0:21:02.87,Main,,0000,0000,0000,,!ميتسكي
Dialogue: 0,0:21:03.20,0:21:03.73,Main,,0000,0000,0000,,!أجل
Dialogue: 0,0:21:06.03,0:21:08.53,Main,,0000,0000,0000,,.يبدو أنّني سأعيقكم فحسب
Dialogue: 0,0:21:09.53,0:21:12.00,Main,,0000,0000,0000,,.لا تقلقوا عليّ والحقوا به
Dialogue: 0,0:21:12.47,0:21:16.17,Main,,0000,0000,0000,,.مفهوم. دع أمر الخلايا لنا
Dialogue: 0,0:21:19.83,0:21:24.17,Main,,0000,0000,0000,,الهجمات الجسديّة وكلّ أساليب \N.الجيتسو لم تؤثّر على ذلك الشّخص
Dialogue: 0,0:21:25.57,0:21:26.87,Main,,0000,0000,0000,,.توخّوا بالغ الحذر
Dialogue: 0,0:21:29.63,0:21:30.40,Main,,0000,0000,0000,,!عن إذنك
Dialogue: 0,0:23:27.28,0:23:29.22,Main,,0000,0000,0000,,!لا أصدّق أنّه هزم جونين قرية السّحاب
Dialogue: 0,0:23:29.28,0:23:31.38,Main,,0000,0000,0000,,،أظنّ أنّه حينما يكون مركّزًا وساعيًا خلف خلايا هاشيراما
Dialogue: 0,0:23:31.45,0:23:33.02,Main,,0000,0000,0000,,!يصبح قويًّا بشكلٍ خياليّ
Dialogue: 0,0:23:33.28,0:23:35.75,Main,,0000,0000,0000,,.إنّه لوحده. سنكون بخير إن تعاونّا
Dialogue: 0,0:23:35.82,0:23:36.75,Main,,0000,0000,0000,,أليس كذلك يا هيروغا؟
Dialogue: 0,0:23:36.82,0:23:39.62,Main,,0000,0000,0000,,.هدفي هو أن أنوّه أساكا عن الخطر
Dialogue: 0,0:23:39.92,0:23:41.52,Main,,0000,0000,0000,,!ولا نيّة لي في التّعاون معكم
Dialogue: 0,0:23:41.88,0:23:44.58,Main,,0000,0000,0000,,:في الحلقة القادمة
Dialogue: 0,0:23:44.72,0:23:45.85,Main,,0000,0000,0000,,"مباراة حتّى الموت"
Dialogue: 0,0:23:44.72,0:23:53.96,nxt ep title,,0000,0000,0000,,{\blur3\fad(300,000)}مباراة حتّى الموت\N\N
Dialogue: 0,0:23:45.92,0:23:47.92,Main,,0000,0000,0000,,!وجدتك أخيرًا
Dialogue: 0,0:23:48.12,0:23:51.05,Main,,0000,0000,0000,,.سآخذ خلايا هاشيراما الآن
