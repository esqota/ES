Dialogue: 0,0:15:25.09,0:15:32.04,Default,,0,0,0,,{\an1\c&H05055C&\3c&H00ffffff&}
Dialogue: 0,0:15:32.04,0:15:39.46,Default,,0,0,0,,{\an9\1c&H00c08000&\3c&H00ffffff&}
Dialogue: 0,0:15:32.04,0:15:39.46,Default,,0,0,0,,{\an1\c&H830EF0&\3c&H00ffffff&}
Dialogue: 0,0:00:15.40,0:00:15.40,Default,,0,0,0,,
Dialogue: 0,0:15:40.12,0:15:41.22,Default,,0,0,0,,توقف عندك
Dialogue: 0,0:15:51.54,0:15:54.31,Default,,0,0,0,,قومو قومو نو - السوط
Dialogue: 0,0:16:01.69,0:16:03.70,Default,,0,0,0,,كل واحد من هؤلاء الرجال قوي لوحده
Dialogue: 0,0:16:16.46,0:16:18.34,Default,,0,0,0,,قبعة القش
Dialogue: 0,0:16:18.86,0:16:21.13,Default,,0,0,0,,قدرته مثيرة للإعجاب , كما العاده
Dialogue: 0,0:16:21.13,0:16:22.81,Default,,0,0,0,,وهذا يذكرني
Dialogue: 0,0:16:22.81,0:16:27.22,Default,,0,0,0,,علي أن أحصل على ظله مرة أخرى , وأعيد أورس للحياة بواسطته
Dialogue: 0,0:16:30.18,0:16:32.79,Default,,0,0,0,,أخرجوا للعب , محاربي الزومبي
Dialogue: 0,0:16:41.62,0:16:47.64,Default,,0,0,0,,هيا يا أتباعي , أقبضوا على قبعة القش
Dialogue: 0,0:16:48.25,0:16:49.55,Default,,0,0,0,,موريا
Dialogue: 0,0:16:49.55,0:16:51.11,Default,,0,0,0,,شخص أخر قوي
Dialogue: 0,0:17:07.28,0:17:07.97,Default,,0,0,0,,خذوا طلقاتكم
Dialogue: 0,0:17:14.79,0:17:15.60,Default,,0,0,0,,هناك المزيد
Dialogue: 0,0:17:16.00,0:17:18.72,Default,,0,0,0,,بالطبع , فهذة ساحة معركة
Dialogue: 0,0:17:18.72,0:17:22.30,Default,,0,0,0,,الأشخاص الذين يموتون يكونون ببساطه إضافة لجيشي
Dialogue: 0,0:17:22.64,0:17:24.13,Default,,0,0,0,,أمسكوا قبعة القش
Dialogue: 0,0:17:24.13,0:17:26.17,Default,,0,0,0,,أنه المسؤول عن الهروب في السجن
Dialogue: 0,0:17:26.17,0:17:28.58,Default,,0,0,0,,تابعوا التقدم
Dialogue: 0,0:17:29.44,0:17:31.91,Default,,0,0,0,,خصومك كلهم أصحاب رتب عالية
Dialogue: 0,0:17:32.65,0:17:35.35,Default,,0,0,0,,ما المدى الذي تظن أنك ستصل إلية ؟
Dialogue: 0,0:17:38.17,0:17:39.42,Default,,0,0,0,,...لوفي
Dialogue: 0,0:17:59.23,0:18:01.36,Default,,0,0,0,,أبتعد لوفي
Dialogue: 0,0:18:17.74,0:18:19.83,Default,,0,0,0,,أنت تعرف هذا بالفعل
Dialogue: 0,0:18:20.26,0:18:22.42,Default,,0,0,0,,أنا وأنت كلانا قراصنة
Dialogue: 0,0:18:22.90,0:18:25.46,Default,,0,0,0,,يفترض أن تكون بعيدا تبحر كما تشاء
Dialogue: 0,0:18:25.46,0:18:28.72,Default,,0,0,0,,أنا , أملك مغامرتي الخاصة
Dialogue: 0,0:18:29.46,0:18:32.60,Default,,0,0,0,,أنا , أملك أصدقاء لي
Dialogue: 0,0:18:33.78,0:18:34.68,Default,,0,0,0,,...أيــس
Dialogue: 0,0:18:34.68,0:18:39.19,Default,,0,0,0,,لا يحق لك التدخل في هذا
Dialogue: 0,0:18:39.78,0:18:42.99,Default,,0,0,0,,...ضعيف مثلك , يأتي لينقذني
Dialogue: 0,0:18:42.99,0:18:43.90,Default,,0,0,0,,أبتعد
Dialogue: 0,0:18:43.90,0:18:47.15,Default,,0,0,0,,هل تظن أني سأسمح بحصول هذا ؟
Dialogue: 0,0:18:47.61,0:18:49.39,Default,,0,0,0,,سيكون هذا مهين جدا
Dialogue: 0,0:18:49.89,0:18:53.32,Default,,0,0,0,,أرجع يالوفي , لماذا أتيت ؟
Dialogue: 0,0:18:55.72,0:18:57.97,Default,,0,0,0,,...أرجوك , لوفي
Dialogue: 0,0:18:57.97,0:19:00.86,Default,,0,0,0,,لا أريد أن تموت معي
Dialogue: 0,0:19:02.31,0:19:05.64,Default,,0,0,0,,هذا خطأي
Dialogue: 0,0:19:07.81,0:19:08.61,Default,,0,0,0,,...أنا
Dialogue: 0,0:19:09.85,0:19:11.21,Default,,0,0,0,,أخـــاك
Dialogue: 0,0:19:18.64,0:19:19.94,Default,,0,0,0,,هل تعرف هذا ؟
Dialogue: 0,0:19:20.59,0:19:23.86,Default,,0,0,0,,عندما نتبادل أكواب الشراب , نصبح أخوة
Dialogue: 0,0:19:25.90,0:19:28.91,Default,,0,0,0,,من الأن وصاعدا , نحن أخوه
Dialogue: 0,0:19:37.45,0:19:40.10,Default,,0,0,0,,..."الأن , لقد قال بدون شك " أخوه
Dialogue: 0,0:19:40.10,0:19:41.71,Default,,0,0,0,,هذا يعني أنه أيضا أبن روجر...؟
Dialogue: 0,0:19:42.20,0:19:44.96,Default,,0,0,0,,والدي أيس ماتا بعد أن ولد
Dialogue: 0,0:19:44.96,0:19:46.63,Default,,0,0,0,,يستحيل أن يكون له أخ أصغر منه
Dialogue: 0,0:19:47.96,0:19:50.42,Default,,0,0,0,,...قوانين القراصنة هذة حمقاء
Dialogue: 0,0:19:50.65,0:19:52.39,Default,,0,0,0,,ولم أسمع بها مطلقا
Dialogue: 0,0:19:52.39,0:19:53.51,Default,,0,0,0,,ألا تفهم ؟
Dialogue: 0,0:19:57.28,0:20:00.31,Default,,0,0,0,,هيا , يا أتباعي الزومبي
Dialogue: 0,0:20:08.75,0:20:10.23,Default,,0,0,0,,ماء البحر ؟
Dialogue: 0,0:20:13.07,0:20:14.43,Default,,0,0,0,,جـيـنـبـي
Dialogue: 0,0:20:14.43,0:20:16.65,Default,,0,0,0,,...موريا , اتباعك الزومبي
Dialogue: 0,0:20:16.65,0:20:18.99,Default,,0,0,0,,أنهم ضعفاء ضد الملح
Dialogue: 0,0:20:20.46,0:20:23.24,Default,,0,0,0,,لوفي , أترك أمر الزومبي لي
Dialogue: 0,0:20:23.24,0:20:23.75,Default,,0,0,0,,أنا أعتمد عليك
Dialogue: 0,0:20:23.75,0:20:25.79,Default,,0,0,0,,جـيـنـبـي , فارس البحار
Dialogue: 0,0:20:25.79,0:20:27.19,Default,,0,0,0,,...أحد أعضاء الشيشيبوكاي السبعه
Dialogue: 0,0:20:27.19,0:20:28.34,Default,,0,0,0,,مالذي تفعلونه ؟
Dialogue: 0,0:20:28.34,0:20:32.17,Default,,0,0,0,,أنه فقط مبتدأ واحد ... لا تدعوه يتحكم بالحرب بهذة الطريقة
Dialogue: 0,0:20:32.17,0:20:34.59,Default,,0,0,0,,أبتعدوا عن طريقي
Dialogue: 0,0:20:34.59,0:20:37.07,Default,,0,0,0,,سوف أحطمه
Dialogue: 0,0:20:42.89,0:20:46.15,Default,,0,0,0,,هذا الرجل سيكون خطير جدا في المستقبل
Dialogue: 0,0:20:46.62,0:20:49.97,Default,,0,0,0,,لقد كبــر مع أخوه الغير حقيقي , أيــس
Dialogue: 0,0:20:50.51,0:20:55.65,Default,,0,0,0,,لكن دمائة الحقيقية تجعله أبــن قائد الثوار , دراقون
Dialogue: 0,0:20:56.00,0:20:57.93,Default,,0,0,0,,قائد الثوار , دراقون
Dialogue: 0,0:20:57.93,0:21:00.09,Default,,0,0,0,,أنه أبن أكثر مجرم مثير للخوف في العالم
Dialogue: 0,0:21:00.09,0:21:03.11,Default,,0,0,0,,وأنا أعرف هذا الشخص
Dialogue: 0,0:21:03.11,0:21:04.91,Default,,0,0,0,,أن والده شخص شهير
Dialogue: 0,0:21:04.91,0:21:06.76,Default,,0,0,0,,لقد قالها علنا
Dialogue: 0,0:21:06.76,0:21:07.63,Default,,0,0,0,,لوفي-سان
Dialogue: 0,0:21:10.04,0:21:11.64,Default,,0,0,0,,لوفي
Dialogue: 0,0:21:11.64,0:21:14.67,Default,,0,0,0,,...معرفه أنك أبن شخص شيطاني , تجعلني
Dialogue: 0,0:21:18.40,0:21:19.76,Default,,0,0,0,,إذا هذا هو الأمر !؟
Dialogue: 0,0:21:20.52,0:21:22.11,Default,,0,0,0,,لقد فهمت الأن
Dialogue: 0,0:21:22.11,0:21:23.40,Default,,0,0,0,,ايها اللعين
Dialogue: 0,0:21:23.92,0:21:26.40,Default,,0,0,0,,قومو قومو نو - المسدس
Dialogue: 0,0:21:36.62,0:21:39.75,Default,,0,0,0,,حــل اللغــز  في مدينة لوق تاون
Dialogue: 0,0:21:40.33,0:21:42.87,Default,,0,0,0,,...هذا هو سبب إنقاذ دراقون له
Dialogue: 0,0:21:42.87,0:21:44.62,Default,,0,0,0,,هل سمعتم هذا للتو ؟
Dialogue: 0,0:21:46.54,0:21:48.36,Default,,0,0,0,,...دراقون ... دراقون
Dialogue: 0,0:21:48.36,0:21:50.05,Default,,0,0,0,,المكتب الرئيسي , المكتب الرئيسي
Dialogue: 0,0:21:50.05,0:21:52.00,Default,,0,0,0,,هنا شابوندي أرخبيل
Dialogue: 0,0:21:52.00,0:21:52.73,Default,,0,0,0,,خبر عاجل
Dialogue: 0,0:21:52.73,0:21:54.96,Default,,0,0,0,,أخبروا العالم
Dialogue: 0,0:21:56.26,0:21:58.13,Default,,0,0,0,,لا يجب أن ندعه يعيش
Dialogue: 0,0:21:59.51,0:22:00.89,Default,,0,0,0,,أقضوا عليه بكل ما نملك
Dialogue: 0,0:22:02.36,0:22:03.68,Default,,0,0,0,,لا بأس
Dialogue: 0,0:22:03.68,0:22:06.20,Default,,0,0,0,,لا مـعـنـى مـن إخـفـاء هـذا بـعـد الأن
Dialogue: 0,0:22:06.20,0:22:10.73,Default,,0,0,0,,بهذة الطباع التي لدى لوفي , فمن المستحيل إخفاء الأمر أكثر من هذا
Dialogue: 0,0:22:12.64,0:22:15.50,Default,,0,0,0,,تربيتهم , معا كأخوة كان خطأ كبير
Dialogue: 0,0:22:16.42,0:22:17.44,Default,,0,0,0,,قير ثيرد
Dialogue: 0,0:22:25.57,0:22:34.44,Default,,0,0,0,,قومو قومو نو , البندقية العملاقة
Dialogue: 0,0:22:41.13,0:22:43.72,Default,,0,0,0,,أيــس
Dialogue: 0,0:22:45.76,0:22:48.76,Default,,0,0,0,,يمكنك قول ما تشاء
Dialogue: 0,0:22:48.76,0:22:51.48,Default,,0,0,0,,سوف أنقذك , حتى لو قـتــلت
Dialogue: 0,0:22:52.90,0:22:54.51,Default,,0,0,0,,هذا الفتى
Dialogue: 0,0:22:54.51,0:22:56.44,Default,,0,0,0,,وحده قبعة القش يستطيع قول هذا له
Dialogue: 0,0:22:56.44,0:22:59.00,Default,,0,0,0,,نحن عائلة أيس
Dialogue: 0,0:22:59.00,0:23:00.40,Default,,0,0,0,,أروهم أننا نستطيع إنقاذة
Dialogue: 0,0:23:02.44,0:23:05.53,Default,,0,0,0,,ماركو , لا تدعه يموت
Dialogue: 0,0:23:06.36,0:23:07.16,Default,,0,0,0,,هذا مفهوم
Dialogue: 0,0:23:14.55,0:23:16.06,Default,,0,0,0,,لوفي
Dialogue: 0,0:23:16.81,0:23:25.21,Default,,0,0,0,,
Dialogue: 0,0:23:25.27,0:23:28.80,Default,,0,0,0,,لوفي وقراصنة الوايت بيرد يواصلون التقدم
Dialogue: 0,0:23:28.80,0:23:32.11,Default,,0,0,0,,عندما يرى أصدقائه يقاتلون مستخدمي قدرات فاكهة الشيطان , الذين يقفون في طريقهم
Dialogue: 0,0:23:32.11,0:23:35.36,Default,,0,0,0,,أيــس يتخذ قرار مهم
Dialogue: 0,0:23:35.36,0:23:38.94,Default,,0,0,0,,وفي هذة الأثناء , البحرية تستعد لتنفيذ خطتهم الجديده
Dialogue: 0,0:23:38.94,0:23:40.74,Default,,0,0,0,,في الحلقة القادمة من ون بيس
Dialogue: 0,0:23:40.74,0:23:42.03,Default,,0,0,0,,المعارك المتتالية
Dialogue: 0,0:23:42.03,0:23:44.99,Default,,0,0,0,,جيش قدرات فاكهة الشيطان ضــد جيش قدرات فاكهة الشيطان
Dialogue: 0,0:23:44.99,0:23:47.11,Default,,0,0,0,,سأصبح ملك القراصنة
Dialogue: 0,0:00:15.40,0:00:15.40,Default,,0,0,0,,
