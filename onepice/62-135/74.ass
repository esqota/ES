Dialogue: 0,0:18:09.24,0:18:11.28,Default,,0,0,0,,!لماذا فعلت هذا؟
Dialogue: 0,0:18:11.53,0:18:15.62,Default,,0,0,0,,!لماذا لم أمت بالمعركة؟
Dialogue: 0,0:18:18.08,0:18:20.16,Default,,0,0,0,,!يا لها من عبارة رائعة
Dialogue: 0,0:18:20.16,0:18:21.92,Default,,0,0,0,,!مختارة بعناية
Dialogue: 0,0:18:21.92,0:18:26.50,Default,,0,0,0,,!تلك العبارة المرة المحزنة و المعذبة هي أعلى شكل للفن
