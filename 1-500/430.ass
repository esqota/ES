Dialogue: 0,0:01:30.30,0:01:35.65,ep title,,0000,0000,0000,,{\fad(500,500)}{\i1}كيلر بي رابودين: الجزء 2{\i0}
Dialogue: 0,0:01:58.40,0:01:59.31,Default,,0000,0000,0000,,!الهجمة البهلوانية
Dialogue: 0,0:02:12.31,0:02:15.81,Default,,0000,0000,0000,,إذًا، يمكن للجينتشوريكي أن يقاتل قليلًا
Dialogue: 0,0:02:15.98,0:02:18.61,Default,,0000,0000,0000,,أستطيع أن أقاتل جيدًا ولكَ أن تراهن، أما أنتَ فضعيفٌ وواهن
Dialogue: 0,0:02:19.36,0:02:21.73,Default,,0000,0000,0000,,...إن كنتَ تعرف أين هو ساسكي
Dialogue: 0,0:02:21.77,0:02:23.06,Default,,0000,0000,0000,,!فأخبرني...
Dialogue: 0,0:02:25.27,0:02:26.27,Default,,0000,0000,0000,,ماذا؟
Dialogue: 0,0:02:30.98,0:02:34.31,Default,,0000,0000,0000,,ساسوري! ديدارا! سأتوقف عند هذا الحد
Dialogue: 0,0:02:34.77,0:02:36.65,Default,,0000,0000,0000,,ماذا تعني بـ"سأتوقف عند هذا الحد"؟
Dialogue: 0,0:02:37.23,0:02:38.94,Default,,0000,0000,0000,,سأغادر الأكاتسكي
Dialogue: 0,0:02:39.86,0:02:41.86,Default,,0000,0000,0000,,ماذا تقول؟
Dialogue: 0,0:02:42.56,0:02:44.27,Default,,0000,0000,0000,,سأغادر مع ناغاتو
Dialogue: 0,0:02:45.69,0:02:46.77,Default,,0000,0000,0000,,!لا تهرب
Dialogue: 0,0:02:47.56,0:02:49.69,Default,,0000,0000,0000,,إذًا، ماذا سنفعل يا صديقي ساسوري؟
Dialogue: 0,0:02:50.19,0:02:51.81,Default,,0000,0000,0000,,أهذا سؤالٌ تسأله؟
Dialogue: 0,0:02:51.98,0:02:54.77,Default,,0000,0000,0000,,!سنقضي عليهم بأنفسنا
Dialogue: 0,0:02:55.06,0:02:56.40,Default,,0000,0000,0000,,نحن اثنان وهم ثلاثة
Dialogue: 0,0:02:56.56,0:02:58.11,Default,,0000,0000,0000,,لا يهم
Dialogue: 0,0:02:58.23,0:03:00.36,Default,,0000,0000,0000,,أظنّكَ محقاًّ، همم
Dialogue: 0,0:03:00.36,0:03:02.36,Default,,0000,0000,0000,,سأريهم فنّي
Dialogue: 0,0:03:04.27,0:03:05.44,Default,,0000,0000,0000,,ما هذا؟
Dialogue: 0,0:03:05.48,0:03:06.61,Default,,0000,0000,0000,,!أنتم
Dialogue: 0,0:03:06.69,0:03:09.27,Default,,0000,0000,0000,,لمَ تتسبّبون في ضجّة في قرية الشلال؟
Dialogue: 0,0:03:12.23,0:03:13.27,Default,,0000,0000,0000,,من هذه؟
Dialogue: 0,0:03:14.44,0:03:16.19,Default,,0000,0000,0000,,!إنّها جينتشوريكي الشيتشيبي
Dialogue: 0,0:03:14.44,0:03:16.19,ps,,0000,0000,0000,,شيتشيبي = ذو الذيول السبعة
Dialogue: 0,0:03:16.40,0:03:17.36,Default,,0000,0000,0000,,حقًا؟
Dialogue: 0,0:03:17.81,0:03:18.86,Default,,0000,0000,0000,,!تشومي
Dialogue: 0,0:03:19.73,0:03:21.31,Default,,0000,0000,0000,,إنهم رفاق يا فوو
Dialogue: 0,0:03:21.31,0:03:23.61,Default,,0000,0000,0000,,رفاق؟ أهم رفاقي؟
Dialogue: 0,0:03:24.31,0:03:27.06,Default,,0000,0000,0000,,!أيتها الجينتشوريكي! إنّهما عدوّان لكِ
Dialogue: 0,0:03:28.94,0:03:31.23,Default,,0000,0000,0000,,لا أفهم ما تقول
Dialogue: 0,0:03:33.40,0:03:34.52,Default,,0000,0000,0000,,بات القتال الآن اثنان ضد أربعة
Dialogue: 0,0:03:34.69,0:03:35.81,Default,,0000,0000,0000,,!حسنًا، فلننسحب
Dialogue: 0,0:03:35.90,0:03:37.94,Default,,0000,0000,0000,,!ماذا؟ حسنًا
Dialogue: 0,0:03:40.23,0:03:41.86,Default,,0000,0000,0000,,!سأنال منكم في المرة القادمة
Dialogue: 0,0:03:42.44,0:03:44.61,Default,,0000,0000,0000,,!ماذا؟ لقد هربا
Dialogue: 0,0:03:51.40,0:03:53.19,Default,,0000,0000,0000,,أنتِ فوو من قرية الشلال، صحيح؟
Dialogue: 0,0:03:53.81,0:03:55.44,Default,,0000,0000,0000,,!اضربي قبضتكِ بقبضتي
Dialogue: 0,0:04:07.48,0:04:09.02,Default,,0000,0000,0000,,ما الأمر؟
Dialogue: 0,0:04:09.11,0:04:10.98,Default,,0000,0000,0000,,أنتَ مريبٌ نوعًا ما أيها السيد
Dialogue: 0,0:04:11.44,0:04:13.06,Default,,0000,0000,0000,,ماذا تقولين يا عزيزتي؟
Dialogue: 0,0:04:13.86,0:04:15.52,Default,,0000,0000,0000,,!هيا، اضربي قبضتكِ بقبضتي
Dialogue: 0,0:04:17.19,0:04:18.19,Default,,0000,0000,0000,,!ما الأمر؟
Dialogue: 0,0:04:19.11,0:04:21.27,Default,,0000,0000,0000,,أنتَ مريبٌ نوعًا ما أيها السيد
Dialogue: 0,0:04:21.27,0:04:22.98,Default,,0000,0000,0000,,ماذا تقولين يا عزيزتي؟
Dialogue: 0,0:04:23.94,0:04:26.23,Default,,0000,0000,0000,,!هيا! اضربي قبضتك بقبضتي
Dialogue: 0,0:04:26.40,0:04:27.23,Default,,0000,0000,0000,,!لا
Dialogue: 0,0:04:27.23,0:04:29.23,Default,,0000,0000,0000,,!هيا اضربي -\N!لا -
Dialogue: 0,0:04:29.31,0:04:31.06,Default,,0000,0000,0000,,!اضربي هكذا -\N!لا -
Dialogue: 0,0:04:31.15,0:04:33.23,Default,,0000,0000,0000,,!اضربي هكذا -\N!لا -
Dialogue: 0,0:04:34.48,0:04:37.27,Default,,0000,0000,0000,,على كل حال... فقط افعلي ما يقوله
Dialogue: 0,0:04:37.44,0:04:39.19,Default,,0000,0000,0000,,!اضربي قبضتكِ بقبضتي
Dialogue: 0,0:04:39.27,0:04:41.27,Default,,0000,0000,0000,,!لا
Dialogue: 0,0:04:50.36,0:04:53.19,Default,,0000,0000,0000,,!انتظري! لديك مهمّة يجب تنفيذها
Dialogue: 0,0:04:53.23,0:04:56.23,Default,,0000,0000,0000,,!لا أنصاع لأوامر شينوبي من قرية أخرى
Dialogue: 0,0:04:56.65,0:05:00.48,Default,,0000,0000,0000,,نحن رفاق. تستطيعين القول بأنّنا أصدقاء حتّى
Dialogue: 0,0:05:00.98,0:05:03.31,Default,,0000,0000,0000,,أصدقاء؟ نحن أصدقاء؟
Dialogue: 0,0:05:04.02,0:05:07.06,Default,,0000,0000,0000,,أجل. لستِ وحدك
Dialogue: 0,0:05:18.02,0:05:20.86,Default,,0000,0000,0000,,!يبدو أنّ فوو قد انسجمت مع غارا
Dialogue: 0,0:05:20.94,0:05:22.27,Default,,0000,0000,0000,,لقد بدأ الأمر بمشكلة
Dialogue: 0,0:05:22.36,0:05:24.06,Default,,0000,0000,0000,,لكن النتيجة مقبولة ولا أرى من معضلة
Dialogue: 0,0:05:24.73,0:05:28.44,Default,,0000,0000,0000,,بيي-أوتشان، أظنّ أن اللوم يقع على لحيتك ونظارتك
Dialogue: 0,0:05:28.81,0:05:31.61,Default,,0000,0000,0000,,!اخرس! فلنذهب إلى التالي
Dialogue: 0,0:05:34.56,0:05:36.56,Default,,0000,0000,0000,,جوبي؟ ماذا يكون؟
Dialogue: 0,0:05:36.90,0:05:39.31,Default,,0000,0000,0000,,!سلاحٌ عملاق استدعته الأكاتسكي
Dialogue: 0,0:05:40.23,0:05:45.19,Default,,0000,0000,0000,,لقد امتصّ تشاكرا البيجو ولهذا السبب لا نستطيع استعمال قوتنا الكاملة
Dialogue: 0,0:05:45.44,0:05:48.56,Default,,0000,0000,0000,,حاليًا، لا يمكن للجوبي أن يستدعي قوته الكاملة أيضًا
Dialogue: 0,0:05:49.27,0:05:51.48,Default,,0000,0000,0000,,فقد قامت أميرة قلعة رابو بختمه
Dialogue: 0,0:05:51.69,0:05:55.40,Default,,0000,0000,0000,,لكنّ الجوبي يستجمع قوّته القصوى ليلة البدر
Dialogue: 0,0:05:55.40,0:05:58.40,Default,,0000,0000,0000,,سيذهبون للقلعة مستهدفين الأميرة
Dialogue: 0,0:05:58.65,0:06:01.90,Default,,0000,0000,0000,,،إن لم يوحد كل الجينتشوريكي قواهم
Dialogue: 0,0:06:01.98,0:06:03.65,Default,,0000,0000,0000,,فلا فرصة لنا أمامهم
Dialogue: 0,0:06:05.23,0:06:09.52,Default,,0000,0000,0000,,سنقوم تسعتنا بحماية الأميرة والقلعة... ياي
Dialogue: 0,0:06:17.77,0:06:19.44,Default,,0000,0000,0000,,اضرب قبضتك بقبضتي
Dialogue: 0,0:06:22.81,0:06:24.90,Default,,0000,0000,0000,,ما بالك؟
Dialogue: 0,0:06:26.11,0:06:27.65,Default,,0000,0000,0000,,!سون غوكو
Dialogue: 0,0:06:28.23,0:06:30.48,Default,,0000,0000,0000,,!أخبره أننا لسنا أعداءً
Dialogue: 0,0:06:31.31,0:06:32.23,Default,,0000,0000,0000,,سمعتَ ما قاله
Dialogue: 0,0:06:33.11,0:06:34.27,Default,,0000,0000,0000,,آسفٌ بشأن هذا
Dialogue: 0,0:06:35.02,0:06:38.27,Default,,0000,0000,0000,,فقد كانت شخصيات مشبوهة تظهر مؤخرًا
Dialogue: 0,0:06:38.94,0:06:40.86,Default,,0000,0000,0000,,لذا فقد حسبتكَ واحدًا منهم
Dialogue: 0,0:06:41.56,0:06:45.15,Default,,0000,0000,0000,,لقد أخبرتك... هذا بسبب لحيتك ونظارتك
Dialogue: 0,0:06:45.44,0:06:47.98,Default,,0000,0000,0000,,لا داعي للتعقيب حول مظهري
Dialogue: 0,0:06:48.94,0:06:50.65,Default,,0000,0000,0000,,ماذا تعني بـ"شخصيات مشبوهة"؟
Dialogue: 0,0:06:52.02,0:06:53.69,Default,,0000,0000,0000,,...لا أدري من هم
Dialogue: 0,0:06:54.44,0:06:56.98,Default,,0000,0000,0000,,لكّنهم كانوا يختطفون أطفالًا من قريتنا
Dialogue: 0,0:07:03.27,0:07:05.48,Default,,0000,0000,0000,,...إنه بالتأكيد يبدو وكأنه يتعقّب فريسةً
Dialogue: 0,0:07:06.11,0:07:07.11,Default,,0000,0000,0000,,!تشومي
Dialogue: 0,0:07:08.11,0:07:09.36,Default,,0000,0000,0000,,!فلنلحق بهم
Dialogue: 0,0:07:09.69,0:07:10.73,Default,,0000,0000,0000,,موافق
Dialogue: 0,0:07:13.27,0:07:14.31,Default,,0000,0000,0000,,!مسحوق القشر
Dialogue: 0,0:07:19.65,0:07:20.90,Default,,0000,0000,0000,,أكان استدعاءً؟
Dialogue: 0,0:07:23.06,0:07:25.11,Default,,0000,0000,0000,,إن تدخّلتم فلن أرحمكم
Dialogue: 0,0:07:26.77,0:07:27.77,Default,,0000,0000,0000,,!ساسكي
Dialogue: 0,0:07:28.23,0:07:32.02,Default,,0000,0000,0000,,ناروتو... هل استطعتَ مغادرة القرية؟
Dialogue: 0,0:07:32.11,0:07:34.77,Default,,0000,0000,0000,,!ماذا عنك؟ ستعود إلى كونوها
Dialogue: 0,0:07:35.40,0:07:37.11,Default,,0000,0000,0000,,!لا أتلقّى الأوامر منك
Dialogue: 0,0:07:37.31,0:07:39.65,Default,,0000,0000,0000,,!هل أنت هو المُختطِف؟
Dialogue: 0,0:07:40.11,0:07:41.40,Default,,0000,0000,0000,,وماذا إن كنت كذلك؟
Dialogue: 0,0:07:41.98,0:07:47.56,Default,,0000,0000,0000,,ساسكي! هل انحدرتَ لدرجة أن تكون تابعًا\Nلأوروتشيمارو وتقوم بمثل هذه الأشياء؟
Dialogue: 0,0:07:48.31,0:07:50.11,Default,,0000,0000,0000,,!ولا تعلم حتّى كيف يشعر إيتاتشي
Dialogue: 0,0:07:50.36,0:07:52.27,Default,,0000,0000,0000,,!إيّاك أن تجرؤ على لفظ اسم إيتاتشي
Dialogue: 0,0:07:52.36,0:07:55.40,Default,,0000,0000,0000,,!لقد انسحب إيتاتشي من الأكاتسكي وهو يبحث عنك
Dialogue: 0,0:07:56.19,0:07:57.19,Default,,0000,0000,0000,,ماذا؟
Dialogue: 0,0:07:57.40,0:07:58.40,Default,,0000,0000,0000,,...انظروا من هنا
Dialogue: 0,0:07:59.15,0:08:00.52,Default,,0000,0000,0000,,...اجتماعٌ للجينتشوريكي
Dialogue: 0,0:08:00.61,0:08:02.61,Default,,0000,0000,0000,,أتساءل ما تنوون فعله
Dialogue: 0,0:08:05.36,0:08:06.40,Default,,0000,0000,0000,,...أوروتشيمارو
Dialogue: 0,0:08:07.02,0:08:13.40,Default,,0000,0000,0000,,...مع أنّكم عاجزون على الأرجح لأنّكم سُلبتم قوّة البيجو خاصتكم
Dialogue: 0,0:08:14.31,0:08:17.31,Default,,0000,0000,0000,,ساسكي-كُن... هل نقضي عليهم؟
Dialogue: 0,0:08:17.48,0:08:20.15,Default,,0000,0000,0000,,أوروتشيمارو... سأتوقف عند هذا الحد
Dialogue: 0,0:08:20.40,0:08:23.40,Default,,0000,0000,0000,,ماذا؟ ماذا تعني بأنّك "ستتوقف عن هذا الحد"؟
Dialogue: 0,0:08:24.31,0:08:25.94,Default,,0000,0000,0000,,سأذهب خلف إيتاتشي
Dialogue: 0,0:08:26.98,0:08:28.11,Default,,0000,0000,0000,,!انتظر
Dialogue: 0,0:08:28.19,0:08:29.19,Default,,0000,0000,0000,,!ناروتو
Dialogue: 0,0:08:29.27,0:08:30.48,Default,,0000,0000,0000,,!لا تنسَ أمر مهمّتنا
Dialogue: 0,0:08:30.90,0:08:32.69,Default,,0000,0000,0000,,!أنت مُدبّر تلكَ الاختطافات
Dialogue: 0,0:08:33.06,0:08:35.31,Default,,0000,0000,0000,,!سأعاقبك الآن
Dialogue: 0,0:08:35.31,0:08:37.56,Default,,0000,0000,0000,,...أيها الجينتشوريكي العاجز
Dialogue: 0,0:08:37.65,0:08:40.27,Default,,0000,0000,0000,,...ما كنتُ لأتحدّث لو كنت مكانك
Dialogue: 0,0:08:40.61,0:08:42.36,Default,,0000,0000,0000,,!تقنية الاستدعاء
Dialogue: 0,0:08:46.52,0:08:48.73,Default,,0000,0000,0000,,إذًا، ماذا ستفعلون؟
Dialogue: 0,0:08:51.11,0:08:52.23,Default,,0000,0000,0000,,!سون غوكو
Dialogue: 0,0:08:59.27,0:09:00.31,Default,,0000,0000,0000,,!أوروتشيمارو-ساما
Dialogue: 0,0:09:00.56,0:09:01.98,Default,,0000,0000,0000,,!أنا بخير
Dialogue: 0,0:09:06.02,0:09:07.98,Default,,0000,0000,0000,,...لقد استنفدت التشاكرا خاصتي
Dialogue: 0,0:09:08.15,0:09:09.15,Default,,0000,0000,0000,,!شوكاكو
Dialogue: 0,0:09:16.44,0:09:17.48,Default,,0000,0000,0000,,!أوروتشيمارو-ساما
Dialogue: 0,0:09:17.69,0:09:19.02,Default,,0000,0000,0000,,!أنا بخير
Dialogue: 0,0:09:19.23,0:09:20.23,Default,,0000,0000,0000,,!دوري
Dialogue: 0,0:09:25.23,0:09:26.44,Default,,0000,0000,0000,,!أوروتشيمارو-ساما
Dialogue: 0,0:09:26.61,0:09:28.02,Default,,0000,0000,0000,,...أنا بخير
Dialogue: 0,0:09:28.23,0:09:29.73,Default,,0000,0000,0000,,!لاريات البيجو
Dialogue: 0,0:09:34.02,0:09:36.06,Default,,0000,0000,0000,,!أوروتشيمارو-ساما
Dialogue: 0,0:09:36.44,0:09:38.36,Default,,0000,0000,0000,,يا إلهي... هذا كثير جدًا حتى بالنسبة لي
Dialogue: 0,0:09:39.27,0:09:41.11,Default,,0000,0000,0000,,!خذ هذه
Dialogue: 0,0:09:47.56,0:09:49.27,Default,,0000,0000,0000,,!ما رأيك؟-\N!لقد نجحنا -
Dialogue: 0,0:09:49.31,0:09:51.02,Default,,0000,0000,0000,,كلّا، ذلك مجرّد جِلدٍ مُنسلخ
Dialogue: 0,0:09:51.36,0:09:56.15,Default,,0000,0000,0000,,مع أنّ قوتهم قد استنفدت، إلّا أنهم يستطيعون\Nتسديد ضرباتٍ قوية عندما يوحّدون قواهم
Dialogue: 0,0:09:56.44,0:09:57.73,Default,,0000,0000,0000,,إنه قوي كالعادة
Dialogue: 0,0:09:58.02,0:09:59.61,Default,,0000,0000,0000,,!لكنه يبدو منهكًا
Dialogue: 0,0:10:00.40,0:10:01.40,Default,,0000,0000,0000,,...أوروتشيمارو-ساما
Dialogue: 0,0:10:01.44,0:10:02.73,Default,,0000,0000,0000,,!سأنال منكم في المرة القادمة
Dialogue: 0,0:10:04.81,0:10:06.27,Default,,0000,0000,0000,,...تخلّصنا من واحد
Dialogue: 0,0:10:07.94,0:10:11.48,Default,,0000,0000,0000,,تعال معنا حتّى نستطيع إحباط مُخطّط الأكاتسكي
Dialogue: 0,0:10:13.61,0:10:14.65,Default,,0000,0000,0000,,حسنًا
Dialogue: 0,0:10:15.36,0:10:17.23,Default,,0000,0000,0000,,سنجتمع مع يوغيتو والآخرين
Dialogue: 0,0:10:22.40,0:10:25.44,Default,,0000,0000,0000,,لا زلتُ عاجزةً عن تحديد موقع جينتشوريكي السانبي ياغورا
Dialogue: 0,0:10:22.40,0:10:25.44,ps,,0000,0000,0000,,سانبي = ذو الذيول الثلاثة
Dialogue: 0,0:10:26.27,0:10:28.44,Default,,0000,0000,0000,,لم أجده في أرض الماء
Dialogue: 0,0:10:28.77,0:10:29.81,Default,,0000,0000,0000,,أيّة أفكار؟
Dialogue: 0,0:10:29.94,0:10:35.31,Default,,0000,0000,0000,,...يُفترض بياغورا أن يتوجّه صوب بعض الأراضي الرطبة في مكانٍ ما
Dialogue: 0,0:10:35.48,0:10:37.06,Default,,0000,0000,0000,,لكن لا أستطيع تأكيد ذلك
Dialogue: 0,0:10:37.23,0:10:41.77,Default,,0000,0000,0000,,حتى لو كنتِ لا تستطيعين ذلك، فنحن بحاجةٍ لجميع الجينتشوريكي، صحيح؟
Dialogue: 0,0:10:42.02,0:10:45.02,Default,,0000,0000,0000,,،أجل، إن لم يوحّد كل الجينتشوريكي قوّتهم
Dialogue: 0,0:10:45.23,0:10:46.69,Default,,0000,0000,0000,,!فلا نستطيع هزيمة الجوبي
Dialogue: 0,0:10:47.15,0:10:50.36,Default,,0000,0000,0000,,...سيشنّ أولئك الأشخاص هجومهم فور أن يصبح القمر بدرًا
Dialogue: 0,0:10:50.36,0:10:52.90,Default,,0000,0000,0000,,مستغلّين الوقت الذي سيضعف فيه الختم
Dialogue: 0,0:10:53.27,0:10:55.44,Default,,0000,0000,0000,,كم بقي لنا من الوقت؟
Dialogue: 0,0:10:55.81,0:10:57.23,Default,,0000,0000,0000,,حوالي سبعة أيام
Dialogue: 0,0:10:57.31,0:10:59.23,Default,,0000,0000,0000,,دعونا ننفصل ونبحث عن ياغورا
Dialogue: 0,0:10:59.61,0:11:04.23,Default,,0000,0000,0000,,وإلّا سنضطر لقتال الأكاتسكي ونحن ننقص فردًا
Dialogue: 0,0:11:04.56,0:11:06.90,Default,,0000,0000,0000,,سننقسم إلى فرقٍ مكوّنةٍ من شخصين ونبحث في كل الاتجاهات الأربعة
Dialogue: 0,0:11:07.44,0:11:08.52,Default,,0000,0000,0000,,!حسنًا يا رفاق
Dialogue: 0,0:11:08.94,0:11:13.81,Default,,0000,0000,0000,,!مهما كانت النتيجة، احرصوا أن تجتمعوا في قلعة رابو بعد سبعة أيام
Dialogue: 0,0:11:14.81,0:11:16.40,Default,,0000,0000,0000,,!هيا بنا يا يوغيتو
Dialogue: 0,0:11:26.74,0:11:27.95,Default,,0000,0000,0000,,أتطلّع للعمل معك
Dialogue: 0,0:11:28.95,0:11:30.41,Default,,0000,0000,0000,,...حسنًا
Dialogue: 0,0:11:42.31,0:11:43.31,Default,,0000,0000,0000,,أيمكنك أن ترى أي شيء؟
Dialogue: 0,0:11:44.02,0:11:45.81,Default,,0000,0000,0000,,لا... ثمّة ضباب في كل مكان
Dialogue: 0,0:11:47.81,0:11:50.35,Default,,0000,0000,0000,,إذًا سأتفقّد المكان بوضع السينين
Dialogue: 0,0:11:58.56,0:11:59.06,Default,,0000,0000,0000,,!ما هذا؟
Dialogue: 0,0:11:59.39,0:12:00.14,Default,,0000,0000,0000,,ما الأمر؟
Dialogue: 0,0:12:00.56,0:12:01.77,Default,,0000,0000,0000,,...مطارِدون من كونوها
Dialogue: 0,0:12:03.73,0:12:05.18,Default,,0000,0000,0000,,!إنهم يبحثون عنّي
Dialogue: 0,0:12:10.52,0:12:11.31,Default,,0000,0000,0000,,!اركب
Dialogue: 0,0:12:15.10,0:12:18.60,Default,,0000,0000,0000,,يبدو أن الجميع يأبى أن يمنح الجينتشوريكي حرّيتهم
Dialogue: 0,0:12:19.18,0:12:19.77,Default,,0000,0000,0000,,...أجل
Dialogue: 0,0:12:20.35,0:12:23.18,Default,,0000,0000,0000,,إنّهم يعاملوننَا وكأننا أوعية خطيرة تحوي البيجو
Dialogue: 0,0:12:23.52,0:12:26.48,Default,,0000,0000,0000,,!لسنا خطيرين ولا البيجو أيضًا
Dialogue: 0,0:12:41.64,0:12:44.18,Default,,0000,0000,0000,,إنه ياغورا، جينتشوريكي السانبي
Dialogue: 0,0:12:46.35,0:12:48.02,Default,,0000,0000,0000,,اضرب قبضتك بقبضتي
Dialogue: 0,0:12:49.35,0:12:49.93,Default,,0000,0000,0000,,أرفض ذلك
Dialogue: 0,0:12:51.35,0:12:53.73,Default,,0000,0000,0000,,!أخبره يا إيسوبو
Dialogue: 0,0:12:54.35,0:12:59.18,Default,,0000,0000,0000,,هذان الاثنان يبدوان مريبين بالتأكيد، لكنّهما جينتشوريكي رفيقان
Dialogue: 0,0:13:00.23,0:13:01.81,Default,,0000,0000,0000,,لا يملك الجينتشوريكي رفاقًا
Dialogue: 0,0:13:03.27,0:13:08.81,Default,,0000,0000,0000,,بمجرّد كونكَ جينتشوريكي فأنت تبثّ الرعب والنقمة والكراهية في الآخرين
Dialogue: 0,0:13:09.68,0:13:12.52,Default,,0000,0000,0000,,أحيانًا ما نُطارد من قِبل أشخاصٍ من قريتنا
Dialogue: 0,0:13:13.93,0:13:15.43,Default,,0000,0000,0000,,!ذلك هو قدرنا كجينتشوريكي
Dialogue: 0,0:13:16.23,0:13:17.56,Default,,0000,0000,0000,,لذا لا أريد أي دورٍ في هذا
Dialogue: 0,0:13:17.68,0:13:19.52,Default,,0000,0000,0000,,سأبقى هنا لوحدي
Dialogue: 0,0:13:20.56,0:13:22.18,Default,,0000,0000,0000,,نحن نحتاج قوتك
Dialogue: 0,0:13:22.39,0:13:23.39,Default,,0000,0000,0000,,!اذهبا من هنا
Dialogue: 0,0:13:23.39,0:13:24.52,Default,,0000,0000,0000,,تعال معنا
Dialogue: 0,0:13:24.56,0:13:26.14,Default,,0000,0000,0000,,ذلك هو قدرك
Dialogue: 0,0:13:28.48,0:13:31.06,Default,,0000,0000,0000,,لا ترغماني على تقبّل أي قدر آخر
Dialogue: 0,0:13:34.93,0:13:36.06,Default,,0000,0000,0000,,أتريدني أن أُرغمه؟
Dialogue: 0,0:13:36.81,0:13:38.31,Default,,0000,0000,0000,,ابقَ خارجًا عن الموضوع
Dialogue: 0,0:13:41.56,0:13:43.85,Default,,0000,0000,0000,,جميعنا جينتشوريكي
Dialogue: 0,0:13:44.64,0:13:48.52,Default,,0000,0000,0000,,!القدر الذي أتحمّله رغمًا عني كوني جينتشوريكي هو أكثر من كافٍ
Dialogue: 0,0:13:51.10,0:13:55.52,Default,,0000,0000,0000,,!سواءٌ أُرغمت عليه أم لا فهذا قدرك
Dialogue: 0,0:13:55.56,0:13:56.52,Default,,0000,0000,0000,,!اخرس
Dialogue: 0,0:13:56.93,0:13:59.27,Default,,0000,0000,0000,,...أيها الأحمق الأخرق
Dialogue: 0,0:13:59.31,0:14:03.52,Default,,0000,0000,0000,,!طالما هو قدرك فعليك التعايش معه
Dialogue: 0,0:14:13.27,0:14:13.73,Default,,0000,0000,0000,,!بيي؟
Dialogue: 0,0:14:16.39,0:14:17.14,Default,,0000,0000,0000,,!أنت
Dialogue: 0,0:14:17.68,0:14:20.35,Default,,0000,0000,0000,,نحن جينتشوريكي مثلك
Dialogue: 0,0:14:20.52,0:14:22.56,Default,,0000,0000,0000,,لا نخافك ولا نمتعضك
Dialogue: 0,0:14:22.60,0:14:24.06,Default,,0000,0000,0000,,ولا نكرهك
Dialogue: 0,0:14:24.56,0:14:26.10,Default,,0000,0000,0000,,اضرب قبضتك بقبضتي
Dialogue: 0,0:14:28.64,0:14:31.60,Default,,0000,0000,0000,,فلنوحّد قوانا
Dialogue: 0,0:14:56.68,0:14:58.64,Default,,0000,0000,0000,,لقد اقتربت ليلة البدر
Dialogue: 0,0:14:58.93,0:15:02.39,Default,,0000,0000,0000,,أنّى لبيي-أوتشان ألّا يكون موجودًا هنا وهو الذي خطّط كل هذا؟
Dialogue: 0,0:15:02.48,0:15:05.56,Default,,0000,0000,0000,,لا ننقص شخصًا واحدًا بل ثلاثة
Dialogue: 0,0:15:06.43,0:15:08.27,Default,,0000,0000,0000,,هل سنستطيع القيام بالأمر؟
Dialogue: 0,0:15:08.48,0:15:10.06,Default,,0000,0000,0000,,على الأرجح لا
Dialogue: 0,0:15:10.77,0:15:13.14,Default,,0000,0000,0000,,ما العمل أيتها العمّة يوغيتو؟
Dialogue: 0,0:15:13.48,0:15:14.43,Default,,0000,0000,0000,,عمة"؟"
Dialogue: 0,0:15:14.43,0:15:17.48,Default,,0000,0000,0000,,أعني... أيتها الأخت يوغيتو
Dialogue: 0,0:15:18.48,0:15:22.31,Default,,0000,0000,0000,,لا خيار أمامنا سوى أن نثق بمجيء بيي والآخرين
Dialogue: 0,0:15:22.52,0:15:23.56,Default,,0000,0000,0000,,ها قد أتوا
Dialogue: 0,0:15:33.39,0:15:34.43,Default,,0000,0000,0000,,!انطلقوا
Dialogue: 0,0:15:38.56,0:15:40.89,Default,,0000,0000,0000,,فلنذهب أيضًا، همم
Dialogue: 0,0:15:41.18,0:15:44.73,Default,,0000,0000,0000,,لا داعي للعجلة. فلننتظر حلول البدر
Dialogue: 0,0:15:48.56,0:15:50.93,Default,,0000,0000,0000,,!إنّ غينين الأكاتسكي يهاجمون
Dialogue: 0,0:15:51.18,0:15:53.56,Default,,0000,0000,0000,,!احموا القلعة! لا تدعوا أيًّا منهم يطأ داخلها
Dialogue: 0,0:15:58.27,0:16:00.39,Default,,0000,0000,0000,,!عنصر الماء: تقنية الفقاعات
Dialogue: 0,0:16:01.06,0:16:04.06,Default,,0000,0000,0000,,!عنصر الحُمم: تقنية الصخور الحارقة
Dialogue: 0,0:16:04.52,0:16:07.02,Default,,0000,0000,0000,,!عنصر الرياح: راسين شوريكين
Dialogue: 0,0:16:17.48,0:16:20.43,Default,,0000,0000,0000,,يبدو أن علينا أن نتدخّل، همم
Dialogue: 0,0:16:20.64,0:16:22.35,Default,,0000,0000,0000,,ذلك ليس ضروريًّا
Dialogue: 0,0:16:28.31,0:16:29.48,Default,,0000,0000,0000,,!تمثال غيدو؟
Dialogue: 0,0:16:29.60,0:16:31.81,Default,,0000,0000,0000,,!سيستخدمونه في الهجوم
Dialogue: 0,0:16:32.10,0:16:35.31,Default,,0000,0000,0000,,لن يكون في كامل قوته، لكنه سيفي بالغرض
Dialogue: 0,0:16:35.48,0:16:37.77,Default,,0000,0000,0000,,مع أنّه لا يمتّ بصلةٍ بالفن
Dialogue: 0,0:16:41.48,0:16:42.48,Default,,0000,0000,0000,,!إنها قنبلة بيجو
Dialogue: 0,0:16:43.23,0:16:46.23,Default,,0000,0000,0000,,سحقًا! لا أستطيع هزيمته في وضعي الحالي
Dialogue: 0,0:17:21.98,0:17:23.48,Default,,0000,0000,0000,,هل الجميع بخير؟
Dialogue: 0,0:17:28.52,0:17:30.56,Default,,0000,0000,0000,,!الآن! انطلقوا
Dialogue: 0,0:17:33.77,0:17:35.06,Default,,0000,0000,0000,,!هذا سيئ
Dialogue: 0,0:17:35.89,0:17:37.39,Default,,0000,0000,0000,,!انطلاقة البخار
Dialogue: 0,0:17:44.98,0:17:45.85,Default,,0000,0000,0000,,!...إنهم
Dialogue: 0,0:17:46.60,0:17:47.77,Default,,0000,0000,0000,,!رفاق
Dialogue: 0,0:17:48.39,0:17:50.89,Default,,0000,0000,0000,,!لا داعي للقلق بما أنّي أتيت
Dialogue: 0,0:17:52.43,0:17:53.48,Default,,0000,0000,0000,,!آسف على التأخير
Dialogue: 0,0:17:53.52,0:17:55.48,Default,,0000,0000,0000,,!لقد تأخرت يا بيي-أوتشان
Dialogue: 0,0:17:55.89,0:17:58.39,Default,,0000,0000,0000,,!لكنك وجدتَ ياغورا السانبي
Dialogue: 0,0:17:58.98,0:18:01.31,Default,,0000,0000,0000,,!أجل! تسعتنا معًا
Dialogue: 0,0:18:02.27,0:18:04.52,Default,,0000,0000,0000,,!حسنًا أيها الجينتشوريكي
Dialogue: 0,0:18:10.77,0:18:12.81,Default,,0000,0000,0000,,!ارفعوا قبضاتكم
Dialogue: 0,0:18:23.52,0:18:24.56,Default,,0000,0000,0000,,!ما هذا؟
Dialogue: 0,0:18:25.68,0:18:26.39,Default,,0000,0000,0000,,إيتشيبي
Dialogue: 0,0:18:26.56,0:18:27.43,Default,,0000,0000,0000,,نيبي
Dialogue: 0,0:18:27.56,0:18:28.39,Default,,0000,0000,0000,,سانبي
Dialogue: 0,0:18:28.60,0:18:29.39,Default,,0000,0000,0000,,يونبي
Dialogue: 0,0:18:29.85,0:18:30.48,Default,,0000,0000,0000,,غوبي
Dialogue: 0,0:18:30.85,0:18:31.56,Default,,0000,0000,0000,,روكوبي
Dialogue: 0,0:18:31.81,0:18:32.85,Default,,0000,0000,0000,,شيتشيبي
Dialogue: 0,0:18:33.39,0:18:34.06,Default,,0000,0000,0000,,هاتشيبي
Dialogue: 0,0:18:34.43,0:18:35.77,Default,,0000,0000,0000,,!كيوبي
Dialogue: 0,0:18:36.77,0:18:40.27,Default,,0000,0000,0000,,!نحن الجينتشوريكي نقف أمامكم
Dialogue: 0,0:18:42.48,0:18:44.64,Default,,0000,0000,0000,,!يبدو هذا فنيًا نوعًا ما
Dialogue: 0,0:18:45.23,0:18:47.93,Default,,0000,0000,0000,,لا يهم الآن بما أنهم قد اجتمعوا جميعًا
Dialogue: 0,0:19:04.35,0:19:05.06,Default,,0000,0000,0000,,...مهلًا
Dialogue: 0,0:19:05.10,0:19:06.77,Default,,0000,0000,0000,,ثمّة قنابل بيجو هناك أيضًا
Dialogue: 0,0:19:06.81,0:19:08.56,Default,,0000,0000,0000,,وماذا في ذلك؟
Dialogue: 0,0:19:20.73,0:19:21.52,Default,,0000,0000,0000,,!شوكاكو
Dialogue: 0,0:19:21.77,0:19:22.43,Default,,0000,0000,0000,,!ماتاتابي
Dialogue: 0,0:19:22.85,0:19:23.77,Default,,0000,0000,0000,,!إيسوبو
Dialogue: 0,0:19:23.89,0:19:24.68,Default,,0000,0000,0000,,!سون غوكو
Dialogue: 0,0:19:25.02,0:19:25.64,Default,,0000,0000,0000,,!كوكوو
Dialogue: 0,0:19:26.06,0:19:26.81,Default,,0000,0000,0000,,!سايكِن
Dialogue: 0,0:19:27.10,0:19:27.85,Default,,0000,0000,0000,,!تشومي
Dialogue: 0,0:19:28.14,0:19:28.85,Default,,0000,0000,0000,,!غيوكي
Dialogue: 0,0:19:29.35,0:19:30.23,Default,,0000,0000,0000,,!كوراما
Dialogue: 0,0:19:42.52,0:19:44.27,Default,,0000,0000,0000,,!هذا سيئ
Dialogue: 0,0:20:00.52,0:20:02.35,Default,,0000,0000,0000,,!لا تحسبوا أنكم قد فزتم
Dialogue: 0,0:20:02.93,0:20:05.52,Default,,0000,0000,0000,,!أقسم أننا سنقضي عليكم في المرة القادمة! همم
Dialogue: 0,0:20:11.64,0:20:13.73,Default,,0000,0000,0000,,أنا منهك
Dialogue: 0,0:20:14.52,0:20:16.60,Default,,0000,0000,0000,,لقد استنفدت التشاكرا خاصتي مجددًا
Dialogue: 0,0:20:18.35,0:20:19.39,Default,,0000,0000,0000,,لقد فزنا
Dialogue: 0,0:20:20.43,0:20:21.48,Default,,0000,0000,0000,,!نجحنا
Dialogue: 0,0:20:22.31,0:20:23.14,Default,,0000,0000,0000,,أجل، نجحنا
Dialogue: 0,0:20:23.31,0:20:29.52,Default,,0000,0000,0000,,لكن لا يمكننا أن نعتبر هذا نصرًا حقيقيًا ما لم نستعِد قوّة البيجو خاصتنا
Dialogue: 0,0:20:30.27,0:20:31.23,Default,,0000,0000,0000,,...وبقول هذا
Dialogue: 0,0:20:32.23,0:20:34.31,Default,,0000,0000,0000,,!لقد أحرزنا اليوم نصرًا كبيرًا
Dialogue: 0,0:22:37.90,0:22:39.90,Default,,0000,0000,0000,,!كارين! استيقظي
Dialogue: 0,0:22:39.94,0:22:42.03,Default,,0000,0000,0000,,!تماسكي يا كارين
Dialogue: 0,0:22:42.07,0:22:43.61,Default,,0000,0000,0000,,!سأذهب لإحضار الوحدة الطبية الآن
Dialogue: 0,0:22:45.98,0:22:50.07,Default,,0000,0000,0000,,...ساسكي، ساسكي، لا بأس
