Dialogue: 0,0:00:00.77,0:00:03.90,Default,,0000,0000,0000,,غرفة التّشريح 3
Dialogue: 0,0:00:25.30,0:00:26.27,Default,,0000,0000,0000,,!غاتسوغا
Dialogue: 0,0:00:34.47,0:00:35.70,Default,,0000,0000,0000,,!أمّي، فوقك
Dialogue: 0,0:00:36.90,0:00:37.70,Default,,0000,0000,0000,,!أعلم
Dialogue: 0,0:00:40.17,0:00:42.23,Default,,0000,0000,0000,,استخدم الجثّة كدرع
Dialogue: 0,0:00:43.60,0:00:45.07,Default,,0000,0000,0000,,توقّعتُ هذا
Dialogue: 0,0:00:45.60,0:00:47.20,Default,,0000,0000,0000,,كانت التّقارير صحيحة
Dialogue: 0,0:00:47.23,0:00:50.27,Default,,0000,0000,0000,,!يبدو أنّ الهجمات الجسديّة هي نقطة ضعفه
Dialogue: 0,0:00:51.47,0:00:53.13,Default,,0000,0000,0000,,معنى هذا أنّنا كعشيرة إينوزوكا
Dialogue: 0,0:00:53.17,0:00:55.00,Default,,0000,0000,0000,,!لن نجد خصماً أنسب
Dialogue: 0,0:00:55.90,0:00:57.97,Default,,0000,0000,0000,,!أمّي، لنحسم الأمر بضربة واحدة
Dialogue: 0,0:00:58.30,0:00:59.40,Default,,0000,0000,0000,,!انتظر يا كيبا
Dialogue: 0,0:00:59.70,0:01:03.50,Default,,0000,0000,0000,,إنّه أحد الستّة الذين هزموا جيرايا-ساما
Dialogue: 0,0:01:04.00,0:01:06.50,Default,,0000,0000,0000,,علينا أن نكون جريئين ويقظين في نفس الوقت
Dialogue: 0,0:01:06.93,0:01:10.70,Default,,0000,0000,0000,,أتوقّع منك أن تحلّ مكان هانا\Nالتي ذهبَتْ لحماية الجِراء
Dialogue: 0,0:01:11.23,0:01:12.93,Default,,0000,0000,0000,,!نـ-نعم
Dialogue: 0,0:01:28.67,0:01:31.50,Default,,0000,0000,0000,,شينو، سنبيدها بكامل قوّتنا
Dialogue: 0,0:01:32.27,0:01:32.97,Default,,0000,0000,0000,,نعم
Dialogue: 0,0:01:33.43,0:01:34.33,Default,,0000,0000,0000,,بيت الشاي\N\N
Dialogue: 0,0:01:34.33,0:01:36.73,Default,,0000,0000,0000,,بيت الشاي\N\N!أنا أتضوّر جوعاً
Dialogue: 0,0:01:37.03,0:01:41.10,Default,,0000,0000,0000,,لنسترح قليلاً، كلّنا مُتعَبون من المهمّة
Dialogue: 0,0:01:44.10,0:01:46.30,Default,,0000,0000,0000,,ما الأمر يا غاي سينسي؟
Dialogue: 0,0:01:47.33,0:01:49.70,Default,,0000,0000,0000,,هل لاحظتُم أعداداً كبيرة من الطيور
Dialogue: 0,0:01:49.73,0:01:52.37,Default,,0000,0000,0000,,تحلّق بعكس جهة القرية؟
Dialogue: 0,0:01:52.77,0:01:54.23,Default,,0000,0000,0000,,كما لو أنّها تلوذ بالفرار
Dialogue: 0,0:01:54.87,0:01:56.90,Default,,0000,0000,0000,,...انتابني هذا الإحساس السيّئ أيضاً
Dialogue: 0,0:01:57.60,0:01:59.67,Default,,0000,0000,0000,,ينبغي أن نتوجّه إلى القرية مباشرة
Dialogue: 0,0:02:00.30,0:02:01.43,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:02:02.03,0:02:03.07,Default,,0000,0000,0000,,!هيّا بنا جميعاً
Dialogue: 0,0:02:05.03,0:02:06.23,Default,,0000,0000,0000,,!تبّاً
Dialogue: 0,0:02:17.53,0:02:18.53,Default,,0000,0000,0000,,!خلفكم
Dialogue: 0,0:02:30.10,0:02:33.77,Default,,0000,0000,0000,,ستّة أقراط في الأنف وسبعة في الأذن... إنّه هو
Dialogue: 0,0:02:35.43,0:02:36.67,Default,,0000,0000,0000,,...أنت
Dialogue: 0,0:02:37.07,0:02:39.37,Default,,0000,0000,0000,,لم أرك منذ وقت طويل يا تسونادي-ساما
Dialogue: 0,0:02:40.33,0:02:42.60,Default,,0000,0000,0000,,أنتِ آخر من بقي من السانين الأسطوري
Dialogue: 0,0:02:43.87,0:02:46.87,Default,,0000,0000,0000,,آمل أن تمنحيني قليلاً من وقتكِ لأتحدّث معكِ
Dialogue: 0,0:04:18.32,0:04:21.32,ep title,,0000,0000,0000,,{\i1}ألم العالم\N\N
Dialogue: 0,0:04:22.75,0:04:24.68,Default,,0000,0000,0000,,!كيبا! سنستخدم تلك التقنيّة
Dialogue: 0,0:04:24.72,0:04:26.28,Default,,0000,0000,0000,,!نعم... أعلم
Dialogue: 0,0:04:27.12,0:04:28.25,Default,,0000,0000,0000,,!لنفعلها يا أكامارو
Dialogue: 0,0:04:30.45,0:04:32.55,Default,,0000,0000,0000,,!التأشير المتحرّك
Dialogue: 0,0:04:37.62,0:04:38.65,Default,,0000,0000,0000,,!لن تفلت
Dialogue: 0,0:04:40.68,0:04:42.18,Default,,0000,0000,0000,,سيطرتُ على الوضع
Dialogue: 0,0:04:43.28,0:04:44.65,Default,,0000,0000,0000,,سأداوي هذين الاثنين
Dialogue: 0,0:04:45.05,0:04:46.62,Default,,0000,0000,0000,,!حسناً، شكراً
Dialogue: 0,0:04:48.62,0:04:50.88,Default,,0000,0000,0000,,{\i1}،ما دامت عليك علامة أكامارو
Dialogue: 0,0:04:50.92,0:04:52.55,Default,,0000,0000,0000,,{\i1}!فلا مفرّ أمامك أبداً
Dialogue: 0,0:04:57.45,0:04:59.12,Default,,0000,0000,0000,,هذه ليست باين
Dialogue: 0,0:05:00.35,0:05:04.18,Default,,0000,0000,0000,,لأنّ سماتها تختلِف عمّا أُبلِغنا عنه
Dialogue: 0,0:05:08.05,0:05:09.25,Default,,0000,0000,0000,,ابقوا متأهّبين
Dialogue: 0,0:05:09.48,0:05:12.88,Default,,0000,0000,0000,,بالطبع، سأبقى يقِظاً مع أي خصم
Dialogue: 0,0:05:26.62,0:05:27.92,Default,,0000,0000,0000,,...نسخ ورقيّة
Dialogue: 0,0:05:29.78,0:05:31.82,Default,,0000,0000,0000,,،مهما كان عدد النسخ كبيراً
Dialogue: 0,0:05:32.55,0:05:34.85,Default,,0000,0000,0000,,!ستلتهمُها خنافسنا كلّها
Dialogue: 0,0:05:40.22,0:05:41.48,Default,,0000,0000,0000,,...هذه الرّائحة
Dialogue: 0,0:05:41.92,0:05:43.05,Default,,0000,0000,0000,,!ابتعد يا شينو
Dialogue: 0,0:05:47.05,0:05:49.32,Default,,0000,0000,0000,,{\i1}فُخِّخَت النّسخ الورقيّة بقنابل ورقيّة من البداية
Dialogue: 0,0:06:02.85,0:06:05.92,Default,,0000,0000,0000,,كانت الأصلية مُشبَعة بالماء
Dialogue: 0,0:06:06.62,0:06:09.38,Default,,0000,0000,0000,,استخدمَتْ ضعفها لكسب أفضليّة
Dialogue: 0,0:06:10.18,0:06:12.18,Default,,0000,0000,0000,,هذه الفتاة قويّة
Dialogue: 0,0:06:19.68,0:06:21.15,Default,,0000,0000,0000,,...أنتَ ذلك الفتى
Dialogue: 0,0:06:28.75,0:06:30.18,Default,,0000,0000,0000,,منذُ زمن بعيد...
Dialogue: 0,0:06:30.98,0:06:32.62,Default,,0000,0000,0000,,يبدو أنّكِ تتذكّرينني
Dialogue: 0,0:06:33.68,0:06:34.78,Default,,0000,0000,0000,,أتعرفينه؟
Dialogue: 0,0:06:35.98,0:06:36.92,Default,,0000,0000,0000,,...قليلاً
Dialogue: 0,0:06:37.42,0:06:38.55,Default,,0000,0000,0000,,من يكون؟
Dialogue: 0,0:06:39.38,0:06:41.48,Default,,0000,0000,0000,,إله يستعيد النّظام
Dialogue: 0,0:06:43.05,0:06:45.45,Default,,0000,0000,0000,,يبدو أنّه ليس في رشده
Dialogue: 0,0:06:46.18,0:06:49.32,Default,,0000,0000,0000,,أوزوماكي ناروتو... أين الكيوبي؟
Dialogue: 0,0:06:50.52,0:06:51.38,Default,,0000,0000,0000,,لا أدري
Dialogue: 0,0:06:52.08,0:06:54.22,Default,,0000,0000,0000,,صيد الجينتشوريكي على وشك أن ينتهي
Dialogue: 0,0:06:55.08,0:07:00.38,Default,,0000,0000,0000,,لم يعد البيجو قائماً على حفظ توازُن\Nالقوى بين قُرى النينجا
Dialogue: 0,0:07:02.65,0:07:06.58,Default,,0000,0000,0000,,حماية الكيوبي الآن باتَت عقيمة
Dialogue: 0,0:07:07.45,0:07:10.08,Default,,0000,0000,0000,,هل هدفك هو ناروتو في نهاية المطاف إذاً؟
Dialogue: 0,0:07:11.42,0:07:13.45,Default,,0000,0000,0000,,ستبدأ الحرب في وقتٍ قريبٍ جدّاً
Dialogue: 0,0:07:15.12,0:07:17.75,Default,,0000,0000,0000,,بذور الحرب منتشرة في\Nكلّ مكان وجاهزة للانفجار
Dialogue: 0,0:07:18.68,0:07:22.38,Default,,0000,0000,0000,,ونحن من سيتحكّم بهذه الحروب
Dialogue: 0,0:07:23.18,0:07:27.32,Default,,0000,0000,0000,,لن نعترض إذا أردتُم التّعاون معنا
Dialogue: 0,0:07:28.32,0:07:30.08,Default,,0000,0000,0000,,...من المشهد الحالي لهذه البلدة
Dialogue: 0,0:07:30.55,0:07:32.88,Default,,0000,0000,0000,,ينبغي أن تكونوا قد أدركتُم\Nحجمَ ما نستطيع فعله
Dialogue: 0,0:07:33.72,0:07:35.42,Default,,0000,0000,0000,,!لا تستهن بالكاغي الخمسة
Dialogue: 0,0:07:37.58,0:07:39.75,Default,,0000,0000,0000,,تسعون أيّها الإرهابيّون لتدمير السّلام
Dialogue: 0,0:07:39.78,0:07:44.08,Default,,0000,0000,0000,,!الذي كافح أسلافُنا لتحقيقه والحفاظ عليه
Dialogue: 0,0:07:44.52,0:07:46.18,Default,,0000,0000,0000,,!لن تجد أذناً صاغية لكلامك
Dialogue: 0,0:07:48.05,0:07:49.22,Default,,0000,0000,0000,,!متعجرفة
Dialogue: 0,0:07:51.22,0:07:52.48,Default,,0000,0000,0000,,{\i1}...يا لها من تشاكرا
Dialogue: 0,0:07:53.02,0:07:54.95,Default,,0000,0000,0000,,{\i1}أهذه هي قوّة الرنّيغان؟
Dialogue: 0,0:07:55.82,0:07:57.42,Default,,0000,0000,0000,,{\i1}لم أرَ مثيلاً لها من قبل
Dialogue: 0,0:07:58.12,0:07:59.42,Default,,0000,0000,0000,,{\i1}...ينتابني إحساس سيّئ
Dialogue: 0,0:08:00.98,0:08:05.12,Default,,0000,0000,0000,,سلامكم قد جرَّ العنف إلينا
Dialogue: 0,0:08:05.75,0:08:07.38,Default,,0000,0000,0000,,أعترف أنّ بعضاً
Dialogue: 0,0:08:07.42,0:08:10.78,Default,,0000,0000,0000,,من تصرّفات قرية الورق السّابقة لم تكن عادلة
Dialogue: 0,0:08:11.65,0:08:14.58,Default,,0000,0000,0000,,!لكن أفعالك هنا لا تُغتفَر
Dialogue: 0,0:08:15.32,0:08:16.92,Default,,0000,0000,0000,,حاذري كلماتكِ
Dialogue: 0,0:08:17.78,0:08:20.68,Default,,0000,0000,0000,,هذا تحذيركِ الأخير من إله
Dialogue: 0,0:08:21.18,0:08:22.72,Default,,0000,0000,0000,,أخبريني أين هو ناروتو
Dialogue: 0,0:08:26.85,0:08:27.78,Default,,0000,0000,0000,,!تكلّمي
Dialogue: 0,0:08:28.45,0:08:32.88,Default,,0000,0000,0000,,!سنبذلُ كلّ ذرّة من قوّتنا لإسقاطك
Dialogue: 0,0:08:33.65,0:08:35.02,Default,,0000,0000,0000,,!هذا كُل ما لدي من كلام
Dialogue: 0,0:08:35.68,0:08:39.52,Default,,0000,0000,0000,,وثمّة شيء آخر... ارتكبتَ خطأً فادحاً
Dialogue: 0,0:08:40.95,0:08:44.65,Default,,0000,0000,0000,,!لن تحصل على مآربك
Dialogue: 0,0:08:46.75,0:08:51.52,Default,,0000,0000,0000,,إن كنتِ تعتقدين أنّ قرية الورق لديها\N...ما يلزم لحماية ناروتو، فأنتِ
Dialogue: 0,0:08:51.55,0:08:52.45,Default,,0000,0000,0000,,!كلّا
Dialogue: 0,0:08:58.48,0:09:01.88,Default,,0000,0000,0000,,ناروتو... قويّ
Dialogue: 0,0:09:06.38,0:09:08.92,Default,,0000,0000,0000,,قسم تفكيك التشفير - قرية الورق المخفيّة
Dialogue: 0,0:09:09.28,0:09:10.62,Default,,0000,0000,0000,,هنالك ستّة من باين
Dialogue: 0,0:09:11.02,0:09:13.58,Default,,0000,0000,0000,,يستطيعُ كلّ واحد منهم استخدام\Nنوع واحد من التقنيّات
Dialogue: 0,0:09:15.22,0:09:17.32,Default,,0000,0000,0000,,استخدمَتْ تلك المرأة تقنيّةَ استدعاء
Dialogue: 0,0:09:18.15,0:09:23.45,Default,,0000,0000,0000,,هذا يعني أنّ باين الذي قُتلَ بواسطة جيرايا-ساما\Nقد تمّ إحياؤه بجسدٍ مختلف
Dialogue: 0,0:09:23.95,0:09:25.68,Default,,0000,0000,0000,,...ولا شكّ
Dialogue: 0,0:09:25.72,0:09:30.32,Default,,0000,0000,0000,,أنّ تلك المرأة قد نُقِلَتْ إلى برج\Nقرية المطر كجثّة
Dialogue: 0,0:09:32.15,0:09:35.35,Default,,0000,0000,0000,,هل جثّة تلك المرأة غرسَتْ هذه في نفسها؟
Dialogue: 0,0:09:35.92,0:09:38.58,Default,,0000,0000,0000,,لا... أنا متأكّد أنّها لم تفعل
Dialogue: 0,0:09:39.88,0:09:41.65,Default,,0000,0000,0000,,هذا يثبت صحّة كلام فوكاساكو-ساما
Dialogue: 0,0:09:42.18,0:09:46.48,Default,,0000,0000,0000,,لدى باين القوّة لإعادة الموتى إلى الحياة
Dialogue: 0,0:09:47.32,0:09:50.55,Default,,0000,0000,0000,,إضافة إلى ذلك، يستطيع تبديل الأجسام\Nكيفما يشاء في أغلب الأوقات
Dialogue: 0,0:09:50.58,0:09:54.08,Default,,0000,0000,0000,,ويتحكّم بها بزرع لواقِط\Nالتشاكرا السوداء هذه
Dialogue: 0,0:09:54.52,0:09:56.85,Default,,0000,0000,0000,,...إذاً باين الستّة أولئك هم
Dialogue: 0,0:09:57.12,0:09:59.42,Default,,0000,0000,0000,,من المُحتمَل أن يكونوا كلّهم جثثاً
Dialogue: 0,0:09:59.85,0:10:03.62,Default,,0000,0000,0000,,وإن كان لديهم لواقِط، فلا بدّ\Nمن وجود أدوات إرسال أيضاً
Dialogue: 0,0:10:04.45,0:10:05.98,Default,,0000,0000,0000,,،إذاً شخص ما في الخفاء
Dialogue: 0,0:10:06.02,0:10:10.42,Default,,0000,0000,0000,,يرسل إشارات التشاكرا ويتحكّم بهم
Dialogue: 0,0:10:10.98,0:10:11.65,Default,,0000,0000,0000,,أجل
Dialogue: 0,0:10:12.98,0:10:15.18,Default,,0000,0000,0000,,،الرّسالة المُشفَّرة التي تركَها جيرايا-ساما
Dialogue: 0,0:10:15.22,0:10:17.08,Default,,0000,0000,0000,,"...الحقيقي ليس بينهم"
Dialogue: 0,0:10:17.12,0:10:21.25,Default,,0000,0000,0000,,ربّما تعني أنّه ولا واحد من الباين الستّة\Nهو باين الحقيقي
Dialogue: 0,0:10:21.68,0:10:24.68,Default,,0000,0000,0000,,!ماذا؟! أتقصدين أنّ هناك آخرين غيرهم؟
Dialogue: 0,0:10:25.08,0:10:26.28,Default,,0000,0000,0000,,بالضبط
Dialogue: 0,0:10:27.62,0:10:29.62,Default,,0000,0000,0000,,{\i1}...توقّعتُ هذا الاحتمال
Dialogue: 0,0:10:29.65,0:10:33.32,Default,,0000,0000,0000,,{\i1}لكن لم أتمنَّ أن يكون صحيحاً\Nكونه السيناريو الأسوأ
Dialogue: 0,0:10:34.65,0:10:37.48,Default,,0000,0000,0000,,وذلك الشّخص يمتلكُ تشاكرا هائلة
Dialogue: 0,0:10:39.08,0:10:41.32,Default,,0000,0000,0000,,فهو قادر على التحكُّم بكلّ هذه الأجسام\Nمن بعيد وكيفما يشاء
Dialogue: 0,0:10:41.35,0:10:42.65,Default,,0000,0000,0000,,باستخدام التشاكرا خاصّته فقط
Dialogue: 0,0:10:42.92,0:10:46.28,Default,,0000,0000,0000,,هذا شبيهٌ بتقنيّة الانتقال الذهني خاصّتنا نحن\Nكعشيرة ياماناكا
Dialogue: 0,0:10:46.62,0:10:49.28,Default,,0000,0000,0000,,لكن عادة ما نستطيع التحكُّم بشخصٍ واحد\Nفقط بالتشاكرا
Dialogue: 0,0:10:49.82,0:10:51.62,Default,,0000,0000,0000,,هنالك ستّة من باين
Dialogue: 0,0:10:52.45,0:10:54.82,Default,,0000,0000,0000,,هل هذا يعني أنّ هناك ستّة آخرين في الخفاء؟
Dialogue: 0,0:10:55.45,0:10:56.98,Default,,0000,0000,0000,,لا نعلم ذلك
Dialogue: 0,0:10:57.48,0:10:59.88,Default,,0000,0000,0000,,على أي حال، إن كانَتْ تقنيّة انتقال ذهنيّ
Dialogue: 0,0:10:59.92,0:11:04.05,Default,,0000,0000,0000,,فلا بدّ أن يكون قريباً جدّاً من اللّاقط\Nكي ينقل التشاكرا
Dialogue: 0,0:11:04.28,0:11:06.88,Default,,0000,0000,0000,,وإلّا فسيكون من المُستحيل شنّ تقنيّات\Nوهجمات بذلك الحجم
Dialogue: 0,0:11:07.35,0:11:10.55,Default,,0000,0000,0000,,لا بدّ وأنّه مختبئ بالقرب من قرية الورق
Dialogue: 0,0:11:10.98,0:11:13.15,Default,,0000,0000,0000,,،علينا أن نبلغ تسونادي-ساما فوراً
Dialogue: 0,0:11:13.18,0:11:15.88,Default,,0000,0000,0000,,وننظّم فريقَ بحث مهمّته إيجاد باين الخفيّ
Dialogue: 0,0:11:16.48,0:11:17.25,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:11:18.58,0:11:20.08,Default,,0000,0000,0000,,{\i1}!كيف لم نلاحظه؟
Dialogue: 0,0:11:30.48,0:11:32.12,Default,,0000,0000,0000,,...لا تتحرّكوا
Dialogue: 0,0:11:34.22,0:11:36.22,Default,,0000,0000,0000,,...أو تموت هذه المرأة
Dialogue: 0,0:11:38.38,0:11:39.45,Default,,0000,0000,0000,,!اللّعنة
Dialogue: 0,0:11:46.35,0:11:48.08,Default,,0000,0000,0000,,...جبل ميوبوكو
Dialogue: 0,0:11:50.42,0:11:51.98,Default,,0000,0000,0000,,!نفس التقنيّة التي يستخدمها أبي
Dialogue: 0,0:11:52.22,0:11:55.35,Default,,0000,0000,0000,,لكن استطاع البحث في عقلِها في غضون\N!ثوانٍ دون أي أداة؟
Dialogue: 0,0:11:57.65,0:12:00.95,Default,,0000,0000,0000,,يبدو أنّ ناروتو ليس هنا
Dialogue: 0,0:12:02.22,0:12:03.55,Default,,0000,0000,0000,,جبل ميوبوكو
Dialogue: 0,0:12:03.98,0:12:04.82,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:12:26.25,0:12:27.02,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:12:27.45,0:12:28.35,Default,,0000,0000,0000,,انسحبوا؟
Dialogue: 0,0:12:32.75,0:12:34.12,Default,,0000,0000,0000,,ما الذي يجري؟
Dialogue: 0,0:12:35.15,0:12:36.55,Default,,0000,0000,0000,,...جبل ميوبوكو
Dialogue: 0,0:12:36.65,0:12:39.02,Default,,0000,0000,0000,,إنّها قرية الضفادع المخفيّة
Dialogue: 0,0:12:40.22,0:12:42.88,Default,,0000,0000,0000,,حسناً إذاً، لا جدوى من البقاء هنا
Dialogue: 0,0:12:43.55,0:12:44.62,Default,,0000,0000,0000,,وكيف عرفت؟
Dialogue: 0,0:12:45.65,0:12:46.52,Default,,0000,0000,0000,,...أيّها الوغد
Dialogue: 0,0:12:48.05,0:12:48.85,Default,,0000,0000,0000,,...تسونادي-ساما
Dialogue: 0,0:12:49.42,0:12:53.52,Default,,0000,0000,0000,,يجدرُ بنا أن ندعهم ينسحبون في الوقت\Nالرّاهن ونحصّن القرية
Dialogue: 0,0:12:55.72,0:12:57.45,Default,,0000,0000,0000,,هلّا طرحتُ سؤالاً أخيراً؟
Dialogue: 0,0:12:59.38,0:13:00.92,Default,,0000,0000,0000,,...تلك التشاكرا في أرجلكم
Dialogue: 0,0:13:01.88,0:13:04.38,Default,,0000,0000,0000,,هل هي للتصدّي لتقنيّتي؟
Dialogue: 0,0:13:05.55,0:13:09.32,Default,,0000,0000,0000,,يبدو أنّكم أدركتُم قدرتي
Dialogue: 0,0:13:09.95,0:13:13.42,Default,,0000,0000,0000,,لكنّها عقيمة أمام القوّة القاهِرة
Dialogue: 0,0:13:14.25,0:13:16.95,Default,,0000,0000,0000,,أثبتُّم أنتُم الدّول العظيمة ذلك\Nعلى مرِّ السنين
Dialogue: 0,0:13:18.98,0:13:23.42,Default,,0000,0000,0000,,كلّكم تعتقدون أنّكم تتبنّون أدواراً قياديّة\N،في هذا العالم
Dialogue: 0,0:13:23.45,0:13:25.55,Default,,0000,0000,0000,,ولا تفكّرون بالموت إطلاقاً
Dialogue: 0,0:13:26.55,0:13:28.52,Default,,0000,0000,0000,,يُطمئنكم السّلام فتصبحون سطحيين
Dialogue: 0,0:13:30.02,0:13:32.38,Default,,0000,0000,0000,,إذا قتلتُم النّاس فسوف تُقتَلون
Dialogue: 0,0:13:34.28,0:13:37.32,Default,,0000,0000,0000,,الكراهيّة هي ما تجلب الفعل وردّة الفعل
Dialogue: 0,0:13:38.62,0:13:40.48,Default,,0000,0000,0000,,كفّ عن الثرثرة بترّهات
Dialogue: 0,0:13:40.85,0:13:45.98,Default,,0000,0000,0000,,في الحروب، يعاني كلا الطرفين من الموت\Nوالجروح والألم
Dialogue: 0,0:13:46.32,0:13:48.92,Default,,0000,0000,0000,,!ونحن الدّول العظيمة نلنا نصيبنا من الألم
Dialogue: 0,0:13:49.45,0:13:52.52,Default,,0000,0000,0000,,!كفّ عن التماس الأعذار لتصرّفاتك
Dialogue: 0,0:13:58.78,0:13:59.95,Default,,0000,0000,0000,,هذا مثيرٌ للضحك
Dialogue: 0,0:14:04.45,0:14:05.48,Default,,0000,0000,0000,,!شيزوني-سان
Dialogue: 0,0:14:06.42,0:14:08.05,Default,,0000,0000,0000,,اشعروا بالألم
Dialogue: 0,0:14:08.88,0:14:10.78,Default,,0000,0000,0000,,تأمّلوا الألم
Dialogue: 0,0:14:17.02,0:14:20.88,Default,,0000,0000,0000,,تقبّلوا الألم، تعرّفوا على الألم
Dialogue: 0,0:14:25.75,0:14:27.15,Default,,0000,0000,0000,,!انتظر يا باين
Dialogue: 0,0:14:28.58,0:14:32.15,Default,,0000,0000,0000,,تقبّلوا الألم، تعرّفوا على الألم
Dialogue: 0,0:14:35.95,0:14:37.88,Default,,0000,0000,0000,,{\i1}أما زال يخطّط لشيء؟
Dialogue: 0,0:14:39.12,0:14:43.02,Default,,0000,0000,0000,,من لا يعرف الألم لا يستطيع أن يفهم\N!السّلام الحقيقي
Dialogue: 0,0:14:51.92,0:14:54.38,Default,,0000,0000,0000,,!أنباء سيّئة! فوكاساكو-ساما
Dialogue: 0,0:14:57.02,0:14:57.92,Default,,0000,0000,0000,,ما الخطب؟
Dialogue: 0,0:14:58.92,0:15:02.12,Default,,0000,0000,0000,,أخي الكبير كوسكي، مبعوثنا، قد قُتِل
Dialogue: 0,0:15:02.55,0:15:03.78,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:15:04.35,0:15:05.72,Default,,0000,0000,0000,,...اسمه
Dialogue: 0,0:15:05.72,0:15:07.65,Default,,0000,0000,0000,,غاماكوسكي    غاماسبيد    أماتشان\N\Nاختفى من السجلّ
Dialogue: 0,0:15:07.65,0:15:09.08,Default,,0000,0000,0000,,غاماسبيد    أماتشان                \N\N
Dialogue: 0,0:15:09.35,0:15:13.12,Default,,0000,0000,0000,,هل هذا يعني أنّ شيئاً قد حلَّ بقرية الورق؟
Dialogue: 0,0:15:13.68,0:15:14.48,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:15:15.12,0:15:18.18,Default,,0000,0000,0000,,الفتى ناروتو، ينتابني إحساسٌ سيّئ
Dialogue: 0,0:15:18.92,0:15:20.22,Default,,0000,0000,0000,,...العجوز الأشيَب
Dialogue: 0,0:15:20.72,0:15:23.12,Default,,0000,0000,0000,,!لا تقل لي أنّ الأكاتسكي قد هاجموا القرية
Dialogue: 0,0:15:23.58,0:15:28.35,Default,,0000,0000,0000,,لحسن الحظّ، ذهبَتْ كا-تشان لجمع المؤونات\Nوهي قريبة من قرية الورق
Dialogue: 0,0:15:28.95,0:15:33.78,Default,,0000,0000,0000,,ستكون قادرة على استدعاء مُحاربين أقوياء\Nبمن فيهم نحن
Dialogue: 0,0:15:34.38,0:15:35.52,Default,,0000,0000,0000,,!استعدّ الآن
Dialogue: 0,0:15:36.02,0:15:36.92,Default,,0000,0000,0000,,!حسناً
Dialogue: 0,0:15:39.52,0:15:41.75,Default,,0000,0000,0000,,مشفى قرية الورق
Dialogue: 0,0:15:44.08,0:15:45.18,Default,,0000,0000,0000,,{\i1}...اللّعنة
Dialogue: 0,0:15:46.05,0:15:48.02,Default,,0000,0000,0000,,{\i1}!لن أستطيع مُداواتهم جميعاً هكذا
Dialogue: 0,0:15:55.45,0:15:56.62,Default,,0000,0000,0000,,!شيزوني-سان
Dialogue: 0,0:16:03.35,0:16:04.25,Default,,0000,0000,0000,,!ماذا؟
Dialogue: 0,0:16:13.45,0:16:14.68,Default,,0000,0000,0000,,ما الذي يجري؟
Dialogue: 0,0:16:15.32,0:16:16.55,Default,,0000,0000,0000,,سأستخدم تلك التقنيّة
Dialogue: 0,0:16:17.08,0:16:17.85,Default,,0000,0000,0000,,ماذا؟
Dialogue: 0,0:16:18.78,0:16:20.22,Default,,0000,0000,0000,,يبدو أنّهم انسحبوا
Dialogue: 0,0:16:21.12,0:16:24.68,Default,,0000,0000,0000,,أجل... لكن هذا غير منطقي
Dialogue: 0,0:16:30.52,0:16:31.62,Default,,0000,0000,0000,,...شيزوني-سان
Dialogue: 0,0:16:37.72,0:16:39.52,Default,,0000,0000,0000,,اختفَتْ رائحتهم تماماً
Dialogue: 0,0:16:39.58,0:16:41.22,Default,,0000,0000,0000,,لم يعودوا هنا
Dialogue: 0,0:16:42.75,0:16:45.85,Default,,0000,0000,0000,,أعتقد أنّ قوّتي قد أرعبَتْهم
Dialogue: 0,0:16:46.82,0:16:50.58,Default,,0000,0000,0000,,دع الأمر لأمّي... كما كان يحدث مع أبي
Dialogue: 0,0:16:53.95,0:16:55.42,Default,,0000,0000,0000,,ما معنى هذا؟
Dialogue: 0,0:16:56.12,0:16:57.65,Default,,0000,0000,0000,,من الصّعب أن أتخيّل أنّهم ينسحبون فحسب
Dialogue: 0,0:17:04.88,0:17:05.78,Default,,0000,0000,0000,,...باين
Dialogue: 0,0:17:07.42,0:17:11.12,Default,,0000,0000,0000,,من لا يعرف الألم لا يستطيع أن يفهم\N!السّلام الحقيقي
Dialogue: 0,0:17:12.88,0:17:14.95,Default,,0000,0000,0000,,...ينوون فعل شيء ما
Dialogue: 0,0:17:15.95,0:17:16.82,Default,,0000,0000,0000,,شيء ما؟
Dialogue: 0,0:17:17.62,0:17:21.52,Default,,0000,0000,0000,,!لا أدري، لكنّه شيء ضخم ما
Dialogue: 0,0:17:23.88,0:17:25.18,Default,,0000,0000,0000,,!أعتمد عليك
Dialogue: 0,0:17:25.92,0:17:28.08,Default,,0000,0000,0000,,!احرص على إيصال الرّسالة إلى كا-تشان
Dialogue: 0,0:17:28.72,0:17:29.45,Default,,0000,0000,0000,,!حاضر
Dialogue: 0,0:17:32.18,0:17:34.15,Default,,0000,0000,0000,,!كاتسويو! احمي الجميع
Dialogue: 0,0:17:35.08,0:17:36.15,Default,,0000,0000,0000,,!تسونادي-ساما
Dialogue: 0,0:17:36.18,0:17:37.85,Default,,0000,0000,0000,,!ثمّة شيء فظيع على وشك أن يحدث
Dialogue: 0,0:17:38.88,0:17:40.22,Default,,0000,0000,0000,,{\i1}ما الذي يؤخِّر ناروتو؟
Dialogue: 0,0:17:42.32,0:17:43.78,Default,,0000,0000,0000,,يجبُ أن أبلغها فوراً
Dialogue: 0,0:17:46.62,0:17:49.25,Default,,0000,0000,0000,,مصدرها قرية الورق
Dialogue: 0,0:17:51.02,0:17:52.85,Default,,0000,0000,0000,,...يا إلهي
Dialogue: 0,0:17:53.88,0:17:54.82,Default,,0000,0000,0000,,!شيما-ساما
Dialogue: 0,0:17:56.02,0:17:58.08,Default,,0000,0000,0000,,أتيتُ عبر بركة الانتقال النُّقَطي
Dialogue: 0,0:17:58.12,0:17:59.68,Default,,0000,0000,0000,,أحمل رسالة من فوكاساكو-ساما
Dialogue: 0,0:18:00.15,0:18:01.62,Default,,0000,0000,0000,,لاستدعائهم، صحيح؟
Dialogue: 0,0:18:15.78,0:18:18.02,Default,,0000,0000,0000,,!باين! ماذا تنوي أن تفعل؟
Dialogue: 0,0:18:20.58,0:18:24.48,Default,,0000,0000,0000,,!لا تفعل! هذه التقنيّة ستقصِّر عمرك
Dialogue: 0,0:18:28.28,0:18:29.68,Default,,0000,0000,0000,,...قوّة الستّة
Dialogue: 0,0:18:30.52,0:18:33.15,Default,,0000,0000,0000,,هل ستفعل هذا حقّاً يا ناغاتو؟
Dialogue: 0,0:18:33.78,0:18:37.08,Default,,0000,0000,0000,,لن أنسى ألم ياهيكو ما حييت
Dialogue: 0,0:18:42.22,0:18:43.38,Default,,0000,0000,0000,,...والآن
Dialogue: 0,0:18:53.18,0:18:55.08,Default,,0000,0000,0000,,سيعرفُ هذا العالم الألم
Dialogue: 0,0:18:58.75,0:19:00.32,Default,,0000,0000,0000,,!تقنيّة الاستدعاء
Dialogue: 0,0:19:15.08,0:19:16.92,Default,,0000,0000,0000,,...الدّفعُ الجبَّار
Dialogue: 0,0:20:13.52,0:20:16.78,Default,,0000,0000,0000,,ما-ما هذا؟
Dialogue: 0,0:20:18.75,0:20:20.62,Default,,0000,0000,0000,,—كيف لهذا أن يحـ
Dialogue: 0,0:20:24.22,0:20:27.52,Default,,0000,0000,0000,,...ناروتو... بسرعة
Dialogue: 0,0:20:30.35,0:20:33.55,Default,,0000,0000,0000,,!أرجوك يا ناروتو
Dialogue: 0,0:22:37.00,0:22:39.73,Default -  top,,0000,0000,0000,,لا داعي لتُزعج هوكاغي قرية الورق نفسها
Dialogue: 0,0:22:39.77,0:22:40.73,Default -  top,,0000,0000,0000,,بأمثالهم
Dialogue: 0,0:22:41.27,0:22:43.87,Default -  top,,0000,0000,0000,,تراجعي واحتسي بعضاً من الشّاي أيتها الجدّة
Dialogue: 0,0:22:44.60,0:22:46.87,Default -  top,,0000,0000,0000,,سأتولّى زمام الأمور من هنا
Dialogue: 0,0:22:47.67,0:22:50.13,Default -  top,,0000,0000,0000,,فقط أخبري كلّ من في القرية\Nألّا يتدخّلوا
Dialogue: 0,0:22:50.77,0:22:54.70,Default -  top,,0000,0000,0000,,سيكون من الصّعب القتال أثناء مُحاولة حمايتهم
Dialogue: 0,0:22:56.00,0:23:00.47,Default -  top,,0000,0000,0000,,:الحلقة القادمة من ناروتو شيبودن\N“انفجار! نمط الناسك”\N\N
Dialogue: 0,0:23:01.00,0:23:02.63,Default -  top,,0000,0000,0000,,!لنحسم هذا الأمر
Dialogue: 0,0:23:05.50,0:23:09.03,Default - tune it again,,0000,0000,0000,,!تابعونا
